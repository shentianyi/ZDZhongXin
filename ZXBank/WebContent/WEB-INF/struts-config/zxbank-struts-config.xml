<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE struts-config PUBLIC "-//Apache Software Foundation//DTD Struts Configuration 1.1//EN" "http://jakarta.apache.org/struts/dtds/struts-config_1_1.dtd">
<struts-config>

	<form-beans>
		<form-bean name="iForm" type="com.zd.csms.zxbank.web.form.CustomerForm"></form-bean>
		<form-bean name="warForm" type="com.zd.csms.zxbank.web.form.WarehouseForm"></form-bean>
		<form-bean name="nForm" type="com.zd.csms.zxbank.web.form.NoticeForm"></form-bean>
		<form-bean name="fiForm" type="com.zd.csms.zxbank.web.form.FinancingForm"></form-bean>
		<form-bean name="agrForm" type="com.zd.csms.zxbank.web.form.AgreementForm"></form-bean>
		<form-bean name="rpForm"
			type="com.zd.csms.zxbank.web.form.RemovePledgeForm"></form-bean>
		<form-bean name="mnForm" type="com.zd.csms.zxbank.web.form.MoveNoticeForm"></form-bean>
		<form-bean name="rForm"
			type="com.zd.csms.zxbank.web.form.ReceivingNoticeForm"></form-bean>
		<form-bean name="gForm" type="com.zd.csms.zxbank.web.form.BankInterfaceForm"></form-bean>
	</form-beans>

	<action-mappings>
		<action path="/ZXinterface" parameter="method" name="iForm"
			type="com.zd.csms.zxbank.web.action.ZXBankInterfaceAction" scope="request">
			<forward name="cuslist" path="/jsp/customer.jsp" />
		</action>
		<action path="/Warehouse" parameter="method" name="warForm"
			type="com.zd.csms.zxbank.web.action.ZXBankInterfaceAction" scope="request">
			<forward name="warehouse" path="/jsp/warehouse.jsp" />
		</action>
		<action path="/ZXnotice" parameter="method" name="nForm"
			type="com.zd.csms.zxbank.web.action.ZXBankInterfaceAction" scope="request">
			<forward name="noticelist" path="/jsp/notice_push.jsp" />
		</action>
		<action path="/Financing" parameter="method" name="fiForm"
			type="com.zd.csms.zxbank.web.action.ZXBankInterfaceAction" scope="request">
			<forward name="financing" path="/jsp/financing.jsp" />
		</action>
		<action path="/agreement" parameter="method" name="agrForm"
			type="com.zd.csms.zxbank.web.action.ZXBankInterfaceAction" scope="request">
			<forward name="agreement" path="/jsp/supervise_agreement.jsp" />
		</action>
		<action path="/removepledge" parameter="method" name="rpForm"
			type="com.zd.csms.zxbank.web.action.ZXBankInterfaceAction" scope="request">
			<forward name="removepledge" path="/jsp/remove_pledge_notice.jsp" />
		</action>
		<action path="/removepledgedetail" parameter="method"
			type="com.zd.csms.zxbank.web.action.ZXBankInterfaceAction" scope="request">
			<forward name="removepledgedetail" path="/jsp/remove_pledge_notice_detail.jsp" />
		</action>
		<action path="/movenotice" parameter="method" name="mnForm"
			type="com.zd.csms.zxbank.web.action.ZXBankInterfaceAction" scope="request">
			<forward name="movenotice" path="/jsp/move_notice.jsp" />
		</action>
		<action path="/movedetail" parameter="method"
			type="com.zd.csms.zxbank.web.action.ZXBankInterfaceAction" scope="request">
			<forward name="movedetail" path="/jsp/move_notice_detail.jsp" />
		</action>
		<action path="/receivingnotice" parameter="method" name="rForm"
			type="com.zd.csms.zxbank.web.action.ZXBankInterfaceAction" scope="request">
			<forward name="receivingnotice" path="/jsp/receiving_notice.jsp" />
		</action>
		<action path="/receivingdetail" parameter="method"
			type="com.zd.csms.zxbank.web.action.ZXBankInterfaceAction" scope="request">
			<forward name="receivingdetail" path="/jsp/receiving_notice_detail.jsp" />
		</action>
		<action path="/gager"  parameter="method" name="gForm"
			type="com.zd.csms.zxbank.web.action.ZXBankInterfaceAction" scope="request">
			<forward name="gager"  path="/jsp/gager_select.jsp" />
			<forward name="gagerApp" path="/jsp/gager_apply.jsp" />
		</action>
		<action path="/commodity"  parameter="method"
			type="com.zd.csms.zxbank.web.action.ZXBankInterfaceAction" scope="request">
			<forward name="commodity"  path="/jsp/gager_select_detail.jsp" />
		</action>
		<action path="/stock"  parameter="method" name="gForm"
			type="com.zd.csms.zxbank.web.action.ZXBankInterfaceAction" scope="request">
			<forward name="stocktaking"  path="/jsp/stock_taking.jsp" />
		</action>
		<action path="/checkstock"  parameter="method" name="gForm"
			type="com.zd.csms.zxbank.web.action.ZXBankInterfaceAction" scope="request">
			<forward name="checkstock"  path="/jsp/stock_taking_select.jsp" />
		</action>
		<action path="/checkstockDetail"  parameter="method"
			type="com.zd.csms.zxbank.web.action.ZXBankInterfaceAction" scope="request">
			<forward name="checkstockDetail"  path="/jsp/stock_taking_detail.jsp" />
		</action>
		<action path="/noticepush"  parameter="method" 
			type="com.zd.csms.zxbank.web.action.ZXBankInterfaceAction" scope="request">
			<forward name="receivingdetail" path="/jsp/receiving_notice_detail.jsp" />
			<forward name="removepledgedetail" path="/jsp/remove_pledge_notice_detail.jsp" />
			<forward name="movedetail" path="/jsp/move_notice_detail.jsp" />
		</action>
	</action-mappings>
</struts-config>	