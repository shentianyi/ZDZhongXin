<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE struts-config PUBLIC "-//Apache Software Foundation//DTD Struts Configuration 1.1//EN" "http://jakarta.apache.org/struts/dtds/struts-config_1_1.dtd">
<struts-config>

	<form-beans>
		<form-bean name="zxbankForm"
			type="com.zd.csms.zxbank.web.form.BankInterfaceForm"></form-bean>
	</form-beans>

	<action-mappings>
		<action path="/ZXinterface" parameter="method" name="zxbankForm"
			type="com.zd.csms.zxbank.web.action.ZXBankInterfaceAction" scope="request">
			<forward name="noticelist" path="/WEB-INF/jsp/zxbank/notice_push.jsp" />
			<forward name="cuslist" path="/WEB-INF/jsp/zxbank/customer.jsp" />
			<forward name="warehouse" path="/WEB-INF/jsp/zxbank/warehouse.jsp" />
			<forward name="financing" path="/WEB-INF/jsp/zxbank/financing.jsp" />
			<forward name="agreement" path="/WEB-INF/jsp/zxbank/supervise_agreement.jsp" />
			<forward name="receivingnotice" path="/WEB-INF/jsp/zxbank/receiving_notice.jsp" />
			<forward name="receivingdetail"
				path="/WEB-INF/jsp/zxbank/receiving_notice_detail.jsp" />
			<forward name="removepledge" path="/WEB-INF/jsp/zxbank/remove_pledge_notice.jsp" />
			<forward name="removepledgedetail"
				path="/WEB-INF/jsp/zxbank/remove_pledge_notice_detail.jsp" />
			<forward name="movenotice" path="/WEB-INF/jsp/zxbank/move_notice.jsp" />
			<forward name="movedetail" path="/WEB-INF/jsp/zxbank/move_notice_detail.jsp" />
			<forward name="gagerApp" path="/WEB-INF/jsp/zxbank/gager_apply.jsp" />
			<forward name="gager" path="/WEB-INF/jsp/zxbank/gager_select.jsp" />
			<forward name="commodity" path="/WEB-INF/jsp/zxbank/gager_select_detail.jsp" />
			<forward name="stocktaking" path="/WEB-INF/jsp/zxbank/stock_taking.jsp" />
			<forward name="checkstock" path="/WEB-INF/jsp/zxbank/stock_taking_select.jsp" />
			<forward name="checkstockDetail" path="/WEB-INF/jsp/zxbank/stock_taking_detail.jsp" />
			<forward name="warehouseUpdate" path="/WEB-INF/jsp/zxbank/warehouse_update.jsp" />
			<forward name="financingupdate" path="/WEB-INF/jsp/zxbank/financing_update.jsp" />
			<forward name="method" path="/WEB-INF/jsp/zxbank/financing_update.jsp"></forward>
			<forward name="noticePushDetail" path="/WEB-INF/jsp/zxbank/notice_push_detail.jsp"></forward>
		</action>
	</action-mappings>
</struts-config>	