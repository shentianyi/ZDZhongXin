<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE struts-config PUBLIC "-//Apache Software Foundation//DTD Struts Configuration 1.1//EN" "http://jakarta.apache.org/struts/dtds/struts-config_1_1.dtd">

<struts-config>

	<form-beans>
		<form-bean name="repositoryForm" type="com.zd.csms.repository.web.form.RepositoryForm"></form-bean>
	</form-beans>
	
  <global-forwards>
    <forward name="error" path="/common/500error.jsp" />
  </global-forwards> 
  
	<action-mappings>
		<action path="/repository" 
		    name="repositoryForm" 
		    parameter="method" 
		    type="com.zd.csms.repository.web.action.RepositoryAction"
		    scope="request">
			<forward name="repository_list" path="/WEB-INF/jsp/repository/repository_list.jsp"/>
			<forward name="add_repository" path="/WEB-INF/jsp/repository/add_repository.jsp"/>
			<forward name="upd_repository" path="/WEB-INF/jsp/repository/upd_repository.jsp"/>
			<forward name="repositoryDispatchCity_list" path="/WEB-INF/jsp/repository/repositoryDispatchCity_list.jsp"/>
			<forward name="add_repository_dispatchCity" path="/WEB-INF/jsp/repository/add_repository_dispatchCity.jsp"/>
			<forward name="upd_repository_dispatchCity" path="/WEB-INF/jsp/repository/upd_repository_dispatchCity.jsp"/>
			<forward name="upd_repositoryTrain" path="/WEB-INF/jsp/repository/upd_repositoryTrain.jsp"/>
			<forward name="repository_detail" path="/WEB-INF/jsp/repository/repository_detail.jsp"/>
			<forward name="select" path="/WEB-INF/jsp/repository/common/select.jsp"></forward>
		</action>
	</action-mappings>
	
</struts-config>