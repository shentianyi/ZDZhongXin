<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE struts-config PUBLIC "-//Apache Software Foundation//DTD Struts Configuration 1.1//EN" "http://jakarta.apache.org/struts/dtds/struts-config_1_1.dtd">

<struts-config>

	<form-beans>
		<form-bean name="importTestForm" type="com.zd.tools.file.importFile.test.ImportForm"></form-bean>
		<form-bean name="testForm" type="com.zd.csms.bank.web.form.BankInterfaceForm"></form-bean>
	</form-beans>

	<global-forwards>
		<forward name="error" path="/common/500error.jsp" />
	</global-forwards> 

	<action-mappings>
		<action path="/test/importFile" 
				name="importTestForm" 
				parameter="method" 
				type="com.zd.tools.file.importFile.test.ImportTestAction"
				scope="request">
				<forward name="preImport" path="/WEB-INF/jsp/test/import_test.jsp"></forward>
		</action>
		
		<action path="/test/moreUser" 
				name="testForm" 
				parameter="method" 
				type="com.zd.csms.bank.web.action.BankInterfaceAction"
				scope="request">
				<forward name="addUser" path="/WEB-INF/jsp/test/addUser.jsp"></forward>
		</action>
	</action-mappings>

</struts-config>

