/*----------------椤电table閿熸枻鎷穞agbar閿熸枻鎷�----------*/
DIV.tagbarMenu{
	position:relative;
	height:80px;
	padding-left:30px;
	background:url(../images/tab-bg.png) repeat-x;
	border-radius:5px 5px 0 0;
}

LABEL.focusLable a,LABEL.blurLable a{
	text-decoration:none;
}
LABEL.focusLable{
	float:left;
	margin-top:35px;
	padding-left:30px;
	padding-right:30px;
	color:#fff;
	text-align:center;
	line-height:45px;
	height:45px;
	display:inline-block;
	font-size:14px;
	background-color:#da3940;
	font-family:'Microsoft YaHei';
	border-radius:5px 5px 0 0;
}

LABEL.blurLable{
	float:left;
	padding-left:30px;
	padding-right:30px;
	margin-top:35px;
	margin-left:3px;
	color:#1c1c1c;
	text-align:center;
	vertical-align:bottom;
	line-height:45px;
	height:45px;
	float:left;
	display:inline-block;
	font-size:14px;
	background-color:#dcdcdc;
	font-family:'Microsoft YaHei';
	border-radius:5px 5px 0 0;
	
	
}


    
.focusLable A:visited {
	COLOR: #fff;
}
.focusLable A:link {
	COLOR: #fff;
}
.focusLable A:hover {
	COLOR: #fff; 
}
.focusLable A:active {
	COLOR: #fff;
}
    
.blurLable A:visited {
	COLOR: #1c1c1c; 
}
.blurLable A:link {
	COLOR: #1c1c1c;
}
.blurLable A:hover {
	COLOR: #1c1c1c;
}
.blurLable A:active {
	COLOR: #8c8c8c;
}

/*----------------Table(Normal)--------------------*/
TABLE.bottomTable{
	padding-bottom:0px;
	padding-left:0px;
	padding-right:0px;
	padding-top:10px; 
	width:100%;
	border:0;
	rainstyle:expression(this.border = "0",
						 this.cellSpacing = "0",
						 this.cellPadding = "0");
}


/*----------------Button閿熸枻鎷烽挳--------------------*/
.formButton {
	margin-right:40px;
	width: 120px;
	height: 40px;
	line-height: 40px;
	color: #fff;
	border: 0;
	background: #c6262d;
	font-family:'Microsoft YaHei';
	cursor:pointer;
	
}

/*----------------Button閿熸枻鎷烽挳--------------------*/
.formButton2{
	BORDER-RIGHT: #e3eff9 0px solid;
	PADDING-RIGHT: 20px;
	BORDER-TOP: #e3eff9 0px solid;
	PADDING-LEFT: 22px;
	FONT-SIZE: 19px;
	FILTER: progid:DXImageTransform.Microsoft.Gradient(GradientType=0, StartColorStr=#fdc04d, EndColorStr=#fdc04d);
	BORDER-LEFT: #e3eff9 0px solid;
	CURSOR: hand;
	COLOR: #FFFFFF;
	PADDING-TOP: 4px;
	PADDING-BOTTOM: 2px;
	BORDER-BOTTOM: #e3eff9 0px solid;
	background-color:#fdc04d;
	outline-style:none; 
	hide-focus:expression(this.hideFocus=true);
	margin-left: 5px;
}

.dividePageButton{
	BORDER-RIGHT: #99A4A6 1px solid;
	PADDING-RIGHT: 5px;
	BORDER-TOP: #99A4A6 1px solid;
	PADDING-LEFT: 5px;
	FONT-SIZE: 12px;
	FILTER: progid:DXImageTransform.Microsoft.Gradient(GradientType=0, StartColorStr=#FFFFFF, EndColorStr=#DFE5E9);
	BORDER-LEFT: #99A4A6 1px solid;
	CURSOR: hand;
	COLOR: #000000;
	PADDING-TOP: 3px;
	PADDING-BOTTOM: 1px;
	BORDER-BOTTOM: #99A4A6 1px solid;
}

