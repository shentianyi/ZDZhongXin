<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xmlns:jee="http://www.springframework.org/schema/jee"
       xsi:schemaLocation="http://www.springframework.org/schema/beans
          http://www.springframework.org/schema/beans/spring-beans-2.0.xsd
          http://www.springframework.org/schema/jee
          http://www.springframework.org/schema/jee/spring-jee-2.0.xsd">

	<bean id="localCacheListener" class="com.zd.core.cache.localcache.LocalCacheListener">
		<property name="wakeUpInterval">
			<value>500</value>
		</property>
		<property name="defaultEviction">
			<bean class="com.zd.core.cache.localcache.LocalCacheEvictionDefault">
				<property name="maxAge">
					<value>300000</value>
				</property>
			</bean>
		</property>
		<property name="evictionList">
			<list>
				<bean class="com.zd.core.cache.localcache.LocalCacheEvictionDefault">
					<property name="name">
						<value>validate</value>
					</property>
					<property name="maxAge">
						<value>0</value>
					</property>
				</bean>
				<bean class="com.zd.core.cache.localcache.LocalCacheEvictionDefault">
					<property name="name">
						<value>menu</value>
					</property>
					<property name="maxAge">
						<value>60000</value>
					</property>
				</bean>
			</list>
		</property>
	</bean>
</beans>


