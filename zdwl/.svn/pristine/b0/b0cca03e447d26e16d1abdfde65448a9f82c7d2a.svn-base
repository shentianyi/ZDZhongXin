package com.zd.csms.business.model;

import java.io.Serializable;
import java.util.Date;

import com.zd.core.annotation.table;

@table(name="t_draft")
public class DraftVO implements Serializable {

	/**
	 * 汇票
	 */
	private static final long serialVersionUID = 1972345668246407919L;

	private int id;
	private String agreement;//质押协议号
	private String bail_num;//保证金账号
	private int distribid;//经销商
	private String financial_institution;//金融机构
	private String brand;//品牌
	private String draft_num;//汇票号码
	private Date billing_date;//开票日
	private Date due_date;//到期日
	private String billing_money;//开票金额
	private String mortgagecar_money;//已押车金额
	private String payment_money;//回款金额
	private int state;//状态  2:未清票
	private Integer createuserid;//创建人
	private Date createdate;//创建时间
	private Integer upduserid;//修改人
	private Date upddate;//修改时间
	private String bailscale;//保证金
	
	/**
	 * @return the bailscale
	 */
	public String getBailscale() {
		return bailscale;
	}
	/**
	 * @param bailscale the bailscale to set
	 */
	public void setBailscale(String bailscale) {
		this.bailscale = bailscale;
	}
	public int getId() {
		return id;
	}
	public void setId(int id) {
		this.id = id;
	}
	public String getAgreement() {
		return agreement;
	}
	public void setAgreement(String agreement) {
		this.agreement = agreement;
	}
	public String getBail_num() {
		return bail_num;
	}
	public void setBail_num(String bailNum) {
		bail_num = bailNum;
	}
	public int getDistribid() {
		return distribid;
	}
	public void setDistribid(int distribid) {
		this.distribid = distribid;
	}
	public String getFinancial_institution() {
		return financial_institution;
	}
	public void setFinancial_institution(String financialInstitution) {
		financial_institution = financialInstitution;
	}
	public String getBrand() {
		return brand;
	}
	public void setBrand(String brand) {
		this.brand = brand;
	}
	public String getDraft_num() {
		return draft_num;
	}
	public void setDraft_num(String draftNum) {
		draft_num = draftNum;
	}
	public Date getBilling_date() {
		return billing_date;
	}
	public void setBilling_date(Date billingDate) {
		billing_date = billingDate;
	}
	public Date getDue_date() {
		return due_date;
	}
	public void setDue_date(Date dueDate) {
		due_date = dueDate;
	}
	public String getBilling_money() {
		return billing_money;
	}
	public void setBilling_money(String billingMoney) {
		billing_money = billingMoney;
	}
	public String getMortgagecar_money() {
		return mortgagecar_money;
	}
	public void setMortgagecar_money(String mortgagecarMoney) {
		mortgagecar_money = mortgagecarMoney;
	}
	public String getPayment_money() {
		return payment_money;
	}
	public void setPayment_money(String paymentMoney) {
		payment_money = paymentMoney;
	}


	public Date getCreatedate() {
		return createdate;
	}
	public void setCreatedate(Date createdate) {
		this.createdate = createdate;
	}

	public Date getUpddate() {
		return upddate;
	}
	public void setUpddate(Date upddate) {
		this.upddate = upddate;
	}

	public Integer getCreateuserid() {
		return createuserid;
	}
	public void setCreateuserid(Integer createuserid) {
		this.createuserid = createuserid;
	}
	public Integer getUpduserid() {
		return upduserid;
	}
	public void setUpduserid(Integer upduserid) {
		this.upduserid = upduserid;
	}
	public int getState() {
		return state;
	}
	public void setState(int state) {
		this.state = state;
	}
	
}
