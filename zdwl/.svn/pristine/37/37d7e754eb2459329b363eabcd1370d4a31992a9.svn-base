package com.zd.csms.marketing.model;

import java.util.Date;

import com.zd.core.annotation.table;

/**
 * 经销商/金融机构绑定信息
 * @author licheng
 *
 */
@table(name="MARKET_BIND_DEALER")
public class BindDealerVO {
	private int id;
	private String dealerId;//经销商Id
	private String dealerName;
	private int repositoryId;//监管员ID
	
	private int supervisorSource;//监管员来源 1：属地 2：外派
	private int supervisorAttribute;//监管员属性 1：新上岗  2： 轮岗 3： 绑定 4：二次上岗 5：暂代
	private String qq;//qq号码
	private String pwd;//密码
	private String laterReason;//晚到原因
	private Date arriveDate;//预计到达时间
	//------------业务部----------------
	
	private Date actualinDate;//实际到店时间
	private String businessDelegate;//业务培训
	private Date trainDate;//培训日期
	private String authorizationBook;//授权书制作
	private String appointmentBook;//委任书制作
	private String traineffect;//培训效果
	
	private int nextApproval;//下一个审批人或填写人
	
	private int approvalState;//审批状态1：通过 2：不通过 3：待审批 4：未提交

	private Integer createUserId;
	private Date createDate;
	
	private String gqpx;//岗前培训
	private String changeMoney;//改变费用
	
	private Integer lastModifyUserId;//最后修改人id
	private Date lastModifyDate;//最后修改时间
	
	private String bankName;//绑定店的所有银行名称 用逗号隔开
	
	public int getId() {
		return id;
	}

	public void setId(int id) {
		this.id = id;
	}



	public String getDealerName() {
		return dealerName;
	}

	public void setDealerName(String dealerName) {
		this.dealerName = dealerName;
	}

	public int getRepositoryId() {
		return repositoryId;
	}

	public void setRepositoryId(int repositoryId) {
		this.repositoryId = repositoryId;
	}

	public int getSupervisorSource() {
		return supervisorSource;
	}

	public void setSupervisorSource(int supervisorSource) {
		this.supervisorSource = supervisorSource;
	}

	public int getSupervisorAttribute() {
		return supervisorAttribute;
	}

	public void setSupervisorAttribute(int supervisorAttribute) {
		this.supervisorAttribute = supervisorAttribute;
	}

	public String getQq() {
		return qq;
	}

	public void setQq(String qq) {
		this.qq = qq;
	}

	public String getPwd() {
		return pwd;
	}

	public void setPwd(String pwd) {
		this.pwd = pwd;
	}

	public String getLaterReason() {
		return laterReason;
	}

	public void setLaterReason(String laterReason) {
		this.laterReason = laterReason;
	}

	public Date getArriveDate() {
		return arriveDate;
	}

	public void setArriveDate(Date arriveDate) {
		this.arriveDate = arriveDate;
	}

	public Date getActualinDate() {
		return actualinDate;
	}

	public void setActualinDate(Date actualinDate) {
		this.actualinDate = actualinDate;
	}

	public String getBusinessDelegate() {
		return businessDelegate;
	}

	public void setBusinessDelegate(String businessDelegate) {
		this.businessDelegate = businessDelegate;
	}

	public Date getTrainDate() {
		return trainDate;
	}

	public void setTrainDate(Date trainDate) {
		this.trainDate = trainDate;
	}

	public String getAuthorizationBook() {
		return authorizationBook;
	}

	public void setAuthorizationBook(String authorizationBook) {
		this.authorizationBook = authorizationBook;
	}

	public String getAppointmentBook() {
		return appointmentBook;
	}

	public void setAppointmentBook(String appointmentBook) {
		this.appointmentBook = appointmentBook;
	}

	public String getTraineffect() {
		return traineffect;
	}

	public void setTraineffect(String traineffect) {
		this.traineffect = traineffect;
	}

	public int getNextApproval() {
		return nextApproval;
	}

	public void setNextApproval(int nextApproval) {
		this.nextApproval = nextApproval;
	}

	public int getApprovalState() {
		return approvalState;
	}

	public void setApprovalState(int approvalState) {
		this.approvalState = approvalState;
	}

	public String getDealerId() {
		return dealerId;
	}

	public void setDealerId(String dealerId) {
		this.dealerId = dealerId;
	}

	public Date getCreateDate() {
		return createDate;
	}

	public void setCreateDate(Date createDate) {
		this.createDate = createDate;
	}

	public String getGqpx() {
		return gqpx;
	}

	public void setGqpx(String gqpx) {
		this.gqpx = gqpx;
	}

	public String getChangeMoney() {
		return changeMoney;
	}

	public void setChangeMoney(String changeMoney) {
		this.changeMoney = changeMoney;
	}

