<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE struts-config PUBLIC "-//Apache Software Foundation//DTD Struts Configuration 1.1//EN" "http://jakarta.apache.org/struts/dtds/struts-config_1_1.dtd">
<struts-config>
	<form-beans>
		<!-- 巡检报告新增修改 -->
		<form-bean name="inspectionForm"
			type="com.zd.csms.windcontrol.web.form.InspectionForm"></form-bean>

	</form-beans>
	<global-forwards>
		<forward name="error" path="/common/500error.jsp" />
	</global-forwards>
	<action-mappings>
		<!--巡检报告 -->
		<action path="/windcontrol/inspection" name="inspectionForm"
			parameter="method" type="com.zd.csms.windcontrol.web.action.InspectionAction"
			scope="request">
			<forward name="add_inspection"
				path="/WEB-INF/jsp/windcontrol/inspection/add_inspection.jsp" />
			<forward name="upd_inspection"
				path="/WEB-INF/jsp/windcontrol/inspection/upd_inspection.jsp" />
			<forward name="view_inspection"
				path="/WEB-INF/jsp/windcontrol/inspection/view_inspection.jsp" />
			<!--打印巡检报告 -->
			<forward name="down_upfile"
				path="/WEB-INF/jsp/windcontrol/inspection/down_upfile.jsp" />

			<!-- 风控部全员及各部门经理以上 :巡检报告列表 -->
			<forward name="viewList"
				path="/WEB-INF/jsp/windcontrol/inspection/viewList.jsp" />
			<!-- 巡视专员 :巡检报告列表 -->
			<forward name="progressList"
				path="/WEB-INF/jsp/windcontrol/inspection/progressList.jsp" />
			<!-- 巡视专员 :巡检报告列表:已完成 -->
			<forward name="completedList"
				path="/WEB-INF/jsp/windcontrol/inspection/completedList.jsp" />

		</action>

	</action-mappings>
</struts-config>