package com.zd.csms.planandreport.web.form;

import org.apache.struts.action.ActionForm;

import com.zd.csms.planandreport.model.InspectionPlanInfoVO;
import com.zd.csms.planandreport.model.InspectionPlanQueryVO;
import com.zd.csms.planandreport.model.InspectionPlanVO;

public class InspectionPlanForm extends ActionForm{

	private static final long serialVersionUID = 1L;
	
	private InspectionPlanVO inspectionPlan = new InspectionPlanVO();
	
	private InspectionPlanInfoVO inspectionPlanInfo = new InspectionPlanInfoVO();
	
	private InspectionPlanQueryVO query = new InspectionPlanQueryVO();

	public InspectionPlanVO getInspectionPlan() {
		return inspectionPlan;
	}

	public void setInspectionPlan(InspectionPlanVO inspectionPlan) {
		this.inspectionPlan = inspectionPlan;
	}

	public InspectionPlanInfoVO getInspectionPlanInfo() {
		return inspectionPlanInfo;
	}

	public void setInspectionPlanInfo(InspectionPlanInfoVO inspectionPlanInfo) {
		this.inspectionPlanInfo = inspectionPlanInfo;
	}

	public InspectionPlanQueryVO getQuery() {
		return query;
	}

	public void setQuery(InspectionPlanQueryVO query) {
		this.query = query;
	}

}
