/* 右侧 测算弹出�? */
.count_area {width:329px;padding:34px 0px 25px 20px;border:#b4b4b4 1px solid;background:url(../images/intention/cout_bg.png) 0px 0px repeat;position:absolute;top:27px;left:2px;z-index:99;display:none;}
.count_area i.close_i {width:11px;height:11px;overflow:hidden;display:block;background:url(../images/intention/intention.png) -55px -91px no-repeat;cursor:pointer;position:absolute;top:4px;right:4px;z-index:20;}
.count_area dl {width:100%;height:32px;padding-bottom:11px;position:relative;z-index:10;}
.count_area dl dt {width:75px;height:32px;line-height:32px;margin-right:3px;display:inline;overflow:hidden;float:left;color:#555;}
.count_area dl dt.spedt {overflow:visible;}
.count_area dl dd {width:222px;height:32px;float:left;}
.count_area dl .ddline {height:30px;border:#c7c7c7 1px solid;}
.count_area dl .focusebg {border:#d19118 1px solid;}
.count_area dl .line1 {width:220px;}
.count_area dl .line2 {width:73px;}
.count_area dl .line3 {width:182px;float:left;}
.ddline input {width:208px;height:16px;line-height:16px;padding:6px 5px;color:#999;border:#f4f4f4 1px solid;background:url(../images/intention/intention.png) 0px -33px repeat-x;float:left;}
.focusebg input {color:#555;}
.fontline input {color:#555;}
.line2 .search_left {width:71px;height:28px;border:#f4f4f4 1px solid;background:url(../images/intention/intention.png) 0px -33px repeat-x;position:relative;}
.line2 .search_left span {width:57px;height:28px;overflow:hidden;line-height:28px;color:#999;float:left;text-align:center;padding:0px;border:none;cursor:default;}
.line2 .search_left a.select_a {width:14px;height:28px;float:right;background:url(../images/intention/intention.png) -79px -62px no-repeat;}
.line2 .search_left .select_brand {width:73px;position:absolute;top:29px;left:-2px;border:#c7c7c7 1px solid;background-color:#fff;z-index:10;}
.line2 .search_left .select_brand li {width:65px;line-height:22px;padding-left:8px;color:#555;cursor:pointer;}
.line2 .search_left .select_brand li.curli {color:#333;background-color:#dfdfdf;}
.line3 input {width:170px;}
.count_area dl a.add_indd {width:32px;height:32px;float:right;background:url(../images/intention/intention.png) -61px 0px no-repeat;}
.count_area .tips_line {width:250px;color:#999;line-height:18px;padding:0px 29px 0px 50px;position:relative;}
.tips_line span {position:absolute;top:0px;left:0px;}
.count_area .btn_line {height:32px;padding:8px 29px 0px 0px;}
.btn_line input {width:55px;height:32px;border:none;background:none;background:url(../images/intention/intention.png) 0px 0px no-repeat;float:right;cursor:pointer;}
.btn_line p {padding-right:25px;line-height:32px;color:#555;float:right;font-size:14px;}
.btn_line p span {color:#209800;font-family:Arial;}
.layoutbg {width:355px;position:absolute;left:0px;top:25px;z-index:10;background-color:#000;filter:alpha(opacity=50);-moz-opacity:0.50;opacity:0.50;display:none;}
.count_area dl .line4 {width:90px;float:left;margin-right:3px;overflow:hidden;display:none;}
.line4 input {width:78px;}
.count_area dl .line5 {width:108px;float:left;margin-right:2px;overflow:hidden;}
.count_area dl .line6 {width:108px;float:left;overflow:hidden;}
.line5 input, .line6 input {width:96px;}
.count_area dl dd.deldd {width:16px;height:20px;cursor:pointer;background:url(../images/intention/deldd_bg.png) 0px 0px no-repeat;margin:5px 0px 0px 6px;}