package com.zd.csms.bank.model;


public class BankQueryVO {
	/**
	 * 查询条件：主键ID
	 */
	private Integer id;
	private String bankName;//银行名称
	private String customerManager;//客户经理
	private String managerPhone;//客户经理电话
	private Integer[] bankType;//银行类型
	private int clientType;	//角色Id
	
	private int parentId;
	private int[] bankChildrenIds;
	
	public Integer getId() {
		return id;
	}
	public void setId(Integer id) {
		this.id = id;
	}
	public String getBankName() {
		return bankName;
	}
	public void setBankName(String bankName) {
		this.bankName = bankName;
	}
	public String getCustomerManager() {
		return customerManager;
	}
	public void setCustomerManager(String customerManager) {
		this.customerManager = customerManager;
	}
	public String getManagerPhone() {
		return managerPhone;
	}
	public void setManagerPhone(String managerPhone) {
		this.managerPhone = managerPhone;
	}
	public Integer[] getBankType() {
		return bankType;
	}
	public void setBankType(Integer[] bankType) {
		this.bankType = bankType;
	}
	public int getParentId() {
		return parentId;
	}
	public void setParentId(int parentId) {
		this.parentId = parentId;
	}
	public int[] getBankChildrenIds() {
		return bankChildrenIds;
	}
	public void setBankChildrenIds(int[] bankChildrenIds) {
		this.bankChildrenIds = bankChildrenIds;
	}
	public int getClientType() {
		return clientType;
	}
	public void setClientType(int clientType) {
		this.clientType = clientType;
	}

	

	
	


	

}
