package com.zd.csms.rbac.web.form;

import org.apache.struts.action.ActionForm;
import com.zd.csms.rbac.model.brand.BrandGroupVO;

public class BrandGroupForm extends ActionForm {

	
	/**
	 * 
	 */
	private static final long serialVersionUID = 1L;
	private BrandGroupVO brandGroup = new BrandGroupVO();
	private int[] brandIds;
	private int[] userIds;
	private String name;
	private String brandName;
	private String userName;


	/**
	 * @return the brandGroup
	 */
	public BrandGroupVO getBrandGroup() {
		return brandGroup;
	}

	/**
	 * @param brandGroup the brandGroup to set
	 */
	public void setBrandGroup(BrandGroupVO brandGroup) {
		this.brandGroup = brandGroup;
	}

	/**
	 * @return the brandIds
	 */
	public int[] getBrandIds() {
		return brandIds;
	}

	/**
	 * @param brandIds the brandIds to set
	 */
	public void setBrandIds(int[] brandIds) {
		this.brandIds = brandIds;
	}

	public int[] getUserIds() {
		return userIds;
	}

	public void setUserIds(int[] userIds) {
		this.userIds = userIds;
	}

	public String getName() {
		return name;
	}

	public void setName(String name) {
		this.name = name;
	}

	/**
	 * @return the brandName
	 */
	public String getBrandName() {
		return brandName;
	}

	/**
	 * @param brandName the brandName to set
	 */
	public void setBrandName(String brandName) {
		this.brandName = brandName;
	}

	public String getUserName() {
		return userName;
	}

	public void setUserName(String userName) {
		this.userName = userName;
	}

}
