package com.zd.csms.marketing.model;

import java.util.Date;

import com.zd.core.annotation.table;

/**
 * 经销商撤店信息流转
 * @author licheng
 *
 */
@table(name="market_dealer_exit")
public class DealerExitVO {
	private int id;//主键ID
	private int dealerId;//经销商主键ID
	private Date exitDateByMarket;//撤店时间
	private int isRefundByMarket;//是否退费 1：退 2：不退
	private String remarkByMarket;//市场部备注
	
	private Date lastPayDate;//最近的一次交费时间
	private String payMoney;//交费金额
	private String payMode;//付费方式
	private int isArrears;//是否欠费 1：欠费 2：不欠费
	private Date arrearsDate;//欠费时间
	private String arrearsNumber;//欠费金额
	private int isRefundByFinance;//是否退费 1：退 2：不退
	private Date exitDateByFinance;//撤店时间
	private String remarkByFinance;//财务部备注
	
	private String businessCommissioner;//业务专员
	private int isBusinessEnd;//业务是否结清 1：结清 2：没结清
	private int endMode;//结清方式 1：清票  2：交接
	private Date exitDateByBusiness;//业务部撤店时间
	private String remarkByBusiness;//业务部备注
	
	private int supervisorId;//储备库ID
	private Date exitDateBySupervise;//监管员管理员撤出时间
	private String remarkBySupervise;//监管员管理部备注
	private int nextApproval;//下一个审批角色
	private int approvalState;//审批状态 1：审批成功 2：审批失败 3：待审批 4:未提交
	private String qq;//监管员QQ
	private String pwd;//监管员密码
	
	private Integer createUserId;//创建人id
	private Integer lastModifyUserId;//最后修改人id
	private Date createDate;//创建时间
	private Date lastModifyDate;//最后修改时间
	
	public int getId() {
		return id;
	}
	public void setId(int id) {
		this.id = id;
	}
	public int getDealerId() {
		return dealerId;
	}
	public void setDealerId(int dealerId) {
		this.dealerId = dealerId;
	}
	public Date getExitDateByMarket() {
		return exitDateByMarket;
	}
	public void setExitDateByMarket(Date exitDateByMarket) {
		this.exitDateByMarket = exitDateByMarket;
	}
	public int getIsRefundByMarket() {
		return isRefundByMarket;
	}
	public void setIsRefundByMarket(int isRefundByMarket) {
		this.isRefundByMarket = isRefundByMarket;
	}
	public Date getCreateDate() {
		return createDate;
	}
	public void setCreateDate(Date createDate) {
		this.createDate = createDate;
	}
	public String getRemarkByMarket() {
		return remarkByMarket;
	}
	public void setRemarkByMarket(String remarkByMarket) {
		this.remarkByMarket = remarkByMarket;
	}
	public Date getLastPayDate() {
		return lastPayDate;
	}
	public void setLastPayDate(Date lastPayDate) {
		this.lastPayDate = lastPayDate;
	}
	public String getPayMoney() {
		return payMoney;
	}
	public void setPayMoney(String payMoney) {
		this.payMoney = payMoney;
	}
	public String getPayMode() {
		return payMode;
	}
	public void setPayMode(String payMode) {
		this.payMode = payMode;
	}
	public int getIsArrears() {
		return isArrears;
	}
	public void setIsArrears(int isArrears) {
		this.isArrears = isArrears;
	}
	public Date getArrearsDate() {
		return arrearsDate;
	}
	public void setArrearsDate(Date arrearsDate) {
		this.arrearsDate = arrearsDate;
	}
	public String getArrearsNumber() {
		return arrearsNumber;
	}
	public void setArrearsNumber(String arrearsNumber) {
		this.arrearsNumber = arrearsNumber;
	}
	public int getIsRefundByFinance() {
		return isRefundByFinance;
	}
	public void setIsRefundByFinance(int isRefundByFinance) {
		this.isRefundByFinance = isRefundByFinance;
	}
	public Date getExitDateByFinance() {
		return exitDateByFinance;
	}
	public void setExitDateByFinance(Date exitDateByFinance) {
		this.exitDateByFinance = exitDateByFinance;
	}
	public String getRemarkByFinance() {
		return remarkByFinance;
	}
	public void setRemarkByFinance(String remarkByFinance) {
		this.remarkByFinance = remarkByFinance;
	}
	public String getBusinessCommissioner() {
		return businessCommissioner;
	}
	public void setBusinessCommissioner(String businessCommissioner) {
		this.businessCommissioner = businessCommissioner;
	}
	public int getIsBusinessEnd() {
		return isBusinessEnd;
	}
	public void setIsBusinessEnd(int isBusinessEnd) {
		this.isBusinessEnd = isBusinessEnd;
	}
	public int getEndMode() {
		return endMode;
	}
	public void setEndMode(int endMode) {
		this.endMode = endMode;
	}
	public Date getExitDateByBusiness() {
		return exitDateByBusiness;
	}
	public void setExitDateByBusiness(Date exitDateByBusiness) {
		this.exitDateByBusiness = exitDateByBusiness;
	}
	public String getRemarkByBusiness() {
		return remarkByBusiness;
	}
	public void setRemarkByBusiness(String remarkByBusiness) {
		this.remarkByBusiness = remarkByBusiness;
	}
	public int getSupervisorId() {
		return supervisorId;
	}
	public void setSupervisorId(int supervisorId) {
		this.supervisorId = supervisorId;
	}
	public Date getExitDateBySupervise() {
		return exitDateBySupervise;
	}
	public void setExitDateBySupervise(Date exitDateBySupervise) {
		this.exitDateBySupervise = exitDateBySupervise;
	}
	public String getRemarkBySupervise() {
		return remarkBySupervise;
	}
	public void setRemarkBySupervise(String remarkBySupervise) {
		this.remarkBySupervise = remarkBySupervise;
	}
	public int getNextApproval() {
		return nextApproval;
	}
	public void setNextApproval(int nextApproval) {
		this.nextApproval = nextApproval;
	}
	public int getApprovalState() {
		return approvalState;
	}
	public void setApprovalState(int approvalState) {
		this.approvalState = approvalState;
	}
	public Integer getLastModifyUserId() {
		return lastModifyUserId;
	}
	public void setLastModifyUserId(Integer lastModifyUserId) {
		this.lastModifyUserId = lastModifyUserId;
	}
	public Date getLastModifyDate() {
		return lastModifyDate;
	}
	public void setLastModifyDate(Date lastModifyDate) {
		this.lastModifyDate = lastModifyDate;
	}
	public void setCreateUserId(Integer createUserId) {
		this.createUserId = createUserId;
	}
	public Integer getCreateUserId() {
		return createUserId;
	}
	public String getQq() {
		return qq;
	}
	public void setQq(String qq) {
		this.qq = qq;
	}
	public String getPwd() {
		return pwd;
	}
	public void setPwd(String pwd) {
		this.pwd = pwd;
	}

	
	
}
