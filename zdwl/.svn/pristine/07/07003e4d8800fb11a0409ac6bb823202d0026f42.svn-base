package com.zd.csms.payment.querybean;

import java.util.Date;

import com.zd.csms.payment.model.PaymentInfoVO;

/**
 * 薪酬查询对象
 * @author wdc
 *
 */
@SuppressWarnings("serial")
public class PaymentInfoQueryBean extends PaymentInfoVO{
	private Date year;//年
	private Date month;//月
	private Integer payState;//薪酬表状态
	public Date getYear() {
		return year;
	}
	public void setYear(Date year) {
		this.year = year;
	}
	public Date getMonth() {
		return month;
	}
	public void setMonth(Date month) {
		this.month = month;
	}
	public Integer getPayState() {
		return payState;
	}
	public void setPayState(Integer payState) {
		this.payState = payState;
	}
}
