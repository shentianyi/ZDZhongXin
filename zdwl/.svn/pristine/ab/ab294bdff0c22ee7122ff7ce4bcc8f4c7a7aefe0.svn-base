package com.zd.csms.supervisorymanagement.web.form;

import org.apache.struts.action.ActionForm;

import com.zd.csms.supervisorymanagement.model.MailcostQueryVO;
import com.zd.csms.supervisorymanagement.model.MailcostVO;

public class MailcostForm extends ActionForm {

	/**
	 * 
	 */
	private static final long serialVersionUID = -4575946697795405253L;

	private MailcostVO mailcost = new MailcostVO();
	private MailcostQueryVO mailcostquery = new MailcostQueryVO();
	private Integer[] mailingitemss;
	
	public MailcostVO getMailcost() {
		return mailcost;
	}
	public void setMailcost(MailcostVO mailcost) {
		this.mailcost = mailcost;
	}
	public MailcostQueryVO getMailcostquery() {
		return mailcostquery;
	}
	public void setMailcostquery(MailcostQueryVO mailcostquery) {
		this.mailcostquery = mailcostquery;
	}
	public Integer[] getMailingitemss() {
		return mailingitemss;
	}
	public void setMailingitemss(Integer[] mailingitemss) {
		this.mailingitemss = mailingitemss;
	}
	
	
}
