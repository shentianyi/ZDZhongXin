package com.zd.csms.rbac.web.form;

import org.apache.struts.action.ActionForm;
import com.zd.csms.rbac.model.DealerGroupVO;

public class DealerGroupForm extends ActionForm {

	/**
	 * 
	 */
	private static final long serialVersionUID = -2369116210349756371L;
	private DealerGroupVO dealerGroup = new DealerGroupVO();
	private int[] dealerIds;
	private int[] userIds;
	private String name;
	private String dealerName;
	private String userName;

	public DealerGroupVO getDealerGroup() {
		return dealerGroup;
	}

	public void setDealerGroup(DealerGroupVO dealerGroup) {
		this.dealerGroup = dealerGroup;
	}

	public int[] getDealerIds() {
		return dealerIds;
	}

	public void setDealerIds(int[] dealerIds) {
		this.dealerIds = dealerIds;
	}

	public int[] getUserIds() {
		return userIds;
	}

	public void setUserIds(int[] userIds) {
		this.userIds = userIds;
	}

	public String getName() {
		return name;
	}

	public void setName(String name) {
		this.name = name;
	}

	public String getDealerName() {
		return dealerName;
	}

	public void setDealerName(String dealerName) {
		this.dealerName = dealerName;
	}

	public String getUserName() {
		return userName;
	}

	public void setUserName(String userName) {
		this.userName = userName;
	}

}
