			<forward name="transfer_supervisor_list" path="/WEB-INF/jsp/transfer/transfer_supervisor_list.jsp"/>
			
		</action>
		<action path="/systemManage" 
		    name="systemManageForm" 
		    parameter="method" 
		    type="com.zd.csms.supervisorymanagement.web.action.SystemManageAction"
		    scope="request">
			<forward name="system_manage_list" path="/WEB-INF/jsp/supervisorymanagement/systemManage/system_manage_list.jsp"/>
			<forward name="add_system_manage" path="/WEB-INF/jsp/supervisorymanagement/systemManage/add_system_manage.jsp"/>
			<forward name="upd_system_manage" path="/WEB-INF/jsp/supervisorymanagement/systemManage/upd_system_manage.jsp"/>
		</action>
		<action path="/systemSupervise" 
		    name="systemSuperviseForm" 
		    parameter="method" 
		    type="com.zd.csms.supervisorymanagement.web.action.SystemSuperviseAction"
		    scope="request">
			<forward name="system_supervise_list" path="/WEB-INF/jsp/supervisorymanagement/systemSupervise/system_supervise_list.jsp"/>
			<forward name="add_system_supervise" path="/WEB-INF/jsp/supervisorymanagement/systemSupervise/add_system_supervise.jsp"/>
			<forward name="upd_system_supervise" path="/WEB-INF/jsp/supervisorymanagement/systemSupervise/upd_system_supervise.jsp"/>
		</action>
		<action path="/mailcost" 
		    name="mailcostForm" 
		    parameter="method" 
		    type="com.zd.csms.supervisorymanagement.web.action.MailcostAction"
		    scope="request">
			<forward name="list_wait_approval" path="/WEB-INF/jsp/supervisorymanagement/mailcost/list_wait_approval.jsp"/>
			<forward name="list_already_approval" path="/WEB-INF/jsp/supervisorymanagement/mailcost/list_already_approval.jsp"/>
			<forward name="detailPage" path="/WEB-INF/jsp/supervisorymanagement/mailcost/detailPage.jsp"/>
			<forward name="approval" path="/WEB-INF/jsp/supervisorymanagement/mailcost/approval.jsp"/>
			<forward name="add_mail_cost" path="/WEB-INF/jsp/supervisorymanagement/mailcost/add_mail_cost.jsp"/>
			<forward name="upd_mail_cost" path="/WEB-INF/jsp/supervisorymanagement/mailcost/upd_mail_cost.jsp"/>
		</action>
		<action path="/dataMailcost" 
		    name="dataMailcostForm" 
		    parameter="method" 
		    type="com.zd.csms.supervisorymanagement.web.action.DataMailcostAction"
		    scope="request">
			<forward name="list_wait_approval" path="/WEB-INF/jsp/supervisorymanagement/datamailcost/list_wait_approval.jsp"/>
			<forward name="list_already_approval" path="/WEB-INF/jsp/supervisorymanagement/datamailcost/list_already_approval.jsp"/>
			<forward name="detailPage" path="/WEB-INF/jsp/supervisorymanagement/datamailcost/detailPage.jsp"/>
			<forward name="approval" path="/WEB-INF/jsp/supervisorymanagement/datamailcost/approval.jsp"/>
			<forward name="add_datamail_cost" path="/WEB-INF/jsp/supervisorymanagement/datamailcost/add_datamail_cost.jsp"/>
			<forward name="upd_datamail_cost" path="/WEB-INF/jsp/supervisorymanagement/datamailcost/upd_datamail_cost.jsp"/>
