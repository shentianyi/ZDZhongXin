			<forward name="HandoverRecordManagementLedgerList" path="/WEB-INF/jsp/handoverLog/handover_record_management_ledger_list.jsp"></forward>
			<forward name="handover_record_management_ledger_detail" path="/WEB-INF/jsp/handoverLog/handover_record_management_ledger_detail.jsp"></forward>
		</action>
		<action path="/transfer" 
		    name="transferForm" 
		    parameter="method" 
		    type="com.zd.csms.supervisorymanagement.web.action.TransferAction"
		    scope="request">
			<forward name="transfer_list" path="/WEB-INF/jsp/transfer/transfer_list.jsp"/>
