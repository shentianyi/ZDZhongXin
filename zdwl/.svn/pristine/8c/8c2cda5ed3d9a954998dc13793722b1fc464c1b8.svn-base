		    name="handoverLogForm" 
		    parameter="method" 
		    type="com.zd.csms.supervisorymanagement.web.action.HandoverLogAction"
		    scope="request">
			<forward name="list_wait_approval" path="/WEB-INF/jsp/handoverLog/handoverLog_list.jsp"/>
			<forward name="list_already_approval" path="/WEB-INF/jsp/handoverLog/handoverLog_already_approval_list.jsp"/>
			<forward name="add_handoverLog" path="/WEB-INF/jsp/handoverLog/add_handoverLog.jsp"/>
			<forward name="upd_handoverLog" path="/WEB-INF/jsp/handoverLog/upd_handoverLog.jsp"/>
			<forward name="handoverLogDetail" path="/WEB-INF/jsp/handoverLog/handoverLog_detail.jsp"/>
			<forward name="down_upfile" path="/WEB-INF/jsp/handoverLog/down_upfile.jsp"/>
			<forward name="upload_handoverLogPic" path="/WEB-INF/jsp/handoverLog/upload_handoverLogPic.jsp"/>
			<forward name="handoverBook_approval" path="/WEB-INF/jsp/handoverLog/handoverBookApproval.jsp"/>
			<forward name="upd_handoverBook" path="/WEB-INF/jsp/handoverLog/upd_handoverBook.jsp"/>
