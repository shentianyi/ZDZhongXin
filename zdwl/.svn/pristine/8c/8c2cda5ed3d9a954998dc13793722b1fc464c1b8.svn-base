		<form-bean name="fixedAssetListForm" type="com.zd.csms.supervisorymanagement.web.form.FixedAssetListForm"></form-bean>
		<form-bean name="fixedAssetsForm" type="com.zd.csms.supervisorymanagement.web.form.FixedAssetsForm"></form-bean>
		<form-bean name="postageRequisitionForm" type="com.zd.csms.supervisorymanagement.web.form.PostageRequisitionForm"></form-bean>
		<form-bean name="handoverLogForm" type="com.zd.csms.supervisorymanagement.web.form.HandoverLogForm"></form-bean>
		<form-bean name="handoverPlanForm" type="com.zd.csms.supervisorymanagement.web.form.HandoverPlanForm"></form-bean>
		<form-bean name="transferForm" type="com.zd.csms.supervisorymanagement.web.form.TransferForm"></form-bean>
		<form-bean name="systemManageForm" type="com.zd.csms.supervisorymanagement.web.form.SystemManageForm"></form-bean>
		<form-bean name="systemSuperviseForm" type="com.zd.csms.supervisorymanagement.web.form.SystemSuperviseForm"></form-bean>
		<form-bean name="mailcostForm" type="com.zd.csms.supervisorymanagement.web.form.MailcostForm"></form-bean>
		<form-bean name="dataMailcostForm" type="com.zd.csms.supervisorymanagement.web.form.DataMailcostForm"></form-bean>
		<form-bean name="leaveApplyForm" type="com.zd.csms.supervisorymanagement.web.form.LeaveApplyForm"></form-bean>
		<form-bean name="extraworkApplyForm" type="com.zd.csms.supervisorymanagement.web.form.ExtraworkApplyForm"></form-bean>
		<form-bean name="resignApplyForm" type="com.zd.csms.supervisorymanagement.web.form.ResignApplyForm"></form-bean>
	</form-beans>
	
  <global-forwards>
    <forward name="error" path="/common/500error.jsp" />
  </global-forwards> 
  
	<action-mappings>
		<action path="/usernameManage" 
		    name="usernameManageForm" 
		    parameter="method" 
		    type="com.zd.csms.supervisorymanagement.web.action.UsernameManageAction"
		    scope="request">
			<forward name="username_manage_list" path="/WEB-INF/jsp/supervisorymanagement/usernameManage/username_manage_list.jsp"/>
			<forward name="add_username_manage" path="/WEB-INF/jsp/supervisorymanagement/usernameManage/add_username_manage.jsp"/>
			<forward name="upd_username_manage" path="/WEB-INF/jsp/supervisorymanagement/usernameManage/upd_username_manage.jsp"/>
		</action>
		<action path="/fixedAssets" 
		    name="fixedAssetsForm" 
		    parameter="method" 
		    type="com.zd.csms.supervisorymanagement.web.action.FixedAssetsAction"
		    scope="request">
			<forward name="fixed_assets_list" path="/WEB-INF/jsp/supervisorymanagement/fixedAssets/fixed_assets_list.jsp"/>
			<forward name="add_fixed_assets" path="/WEB-INF/jsp/supervisorymanagement/fixedAssets/add_fixed_assets.jsp"/>
			<forward name="upd_fixed_assets" path="/WEB-INF/jsp/supervisorymanagement/fixedAssets/upd_fixed_assets.jsp"/>
			<forward name="import_fixed" path="/WEB-INF/jsp/supervisorymanagement/fixedAssets/import_fixed.jsp"/>
		</action>
		<action path="/fixedAssetList" 
		    name="fixedAssetListForm" 
		    parameter="method" 
		    type="com.zd.csms.supervisorymanagement.web.action.FixedAssetListAction"
		    scope="request">
