			<forward name="add_resign_apply" path="/WEB-INF/jsp/supervisorymanagement/resignApply/add_resign_apply.jsp"/>
			<forward name="upd_resign_apply" path="/WEB-INF/jsp/supervisorymanagement/resignApply/upd_resign_apply.jsp"/>
		</action>
	</action-mappings>
	
</struts-config>