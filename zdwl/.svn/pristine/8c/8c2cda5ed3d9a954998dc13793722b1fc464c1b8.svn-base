			<forward name="upd_fixed_asset_list" path="/WEB-INF/jsp/supervisorymanagement/fixedAssets/upd_fixed_asset_list.jsp"/>
			<forward name="detail_fixed_asset_list" path="/WEB-INF/jsp/supervisorymanagement/fixedAssets/detail_fixed_asset_list.jsp"/>
		</action>
		<action path="/postageRequisition" 
		    name="postageRequisitionForm" 
		    parameter="method" 
		    type="com.zd.csms.supervisorymanagement.web.action.PostageRequisitionAction"
		    scope="request">
