			<forward name="list_already_approval" path="/WEB-INF/jsp/supervisorymanagement/leaveApply/list_already_approval.jsp"/>
			<forward name="detailPage" path="/WEB-INF/jsp/supervisorymanagement/leaveApply/detailPage.jsp"/>
			<forward name="approval" path="/WEB-INF/jsp/supervisorymanagement/leaveApply/approval.jsp"/>
			<forward name="add_leave_apply" path="/WEB-INF/jsp/supervisorymanagement/leaveApply/add_leave_apply.jsp"/>
			<forward name="upd_leave_apply" path="/WEB-INF/jsp/supervisorymanagement/leaveApply/upd_leave_apply.jsp"/>
		</action>
		<!-- 加班申请 -->
		<action path="/extraworkApply" 
		    name="extraworkApplyForm" 
		    parameter="method" 
		    type="com.zd.csms.supervisorymanagement.web.action.ExtraworkApplyAction"
		    scope="request">
			<forward name="list_wait_approval" path="/WEB-INF/jsp/supervisorymanagement/extraworkApply/list_wait_approval.jsp"/>
			<forward name="list_already_approval" path="/WEB-INF/jsp/supervisorymanagement/extraworkApply/list_already_approval.jsp"/>
			<forward name="detailPage" path="/WEB-INF/jsp/supervisorymanagement/extraworkApply/detailPage.jsp"/>
			<forward name="approval" path="/WEB-INF/jsp/supervisorymanagement/extraworkApply/approval.jsp"/>
			<forward name="add_extrawork_apply" path="/WEB-INF/jsp/supervisorymanagement/extraworkApply/add_extrawork_apply.jsp"/>
			<forward name="upd_extrawork_apply" path="/WEB-INF/jsp/supervisorymanagement/extraworkApply/upd_extrawork_apply.jsp"/>
		</action>
		<!-- 辞职申请 -->
		<action path="/resignApply" 
		    name="resignApplyForm" 
		    parameter="method" 
		    type="com.zd.csms.supervisorymanagement.web.action.ResignApplyAction"
		    scope="request">
