            <forward name="dataMailingFee_detail" path="/WEB-INF/jsp/supervisorymanagement/datamailcost/dataMailingFee_detail.jsp"></forward>
		</action>
		<action path="/handoverPlan" 
		    name="handoverPlanForm" 
		    parameter="method" 
		    type="com.zd.csms.supervisorymanagement.web.action.HandoverPlanAction"
		    scope="request">
			<forward name="list_wait_approval" path="/WEB-INF/jsp/handoverPlan/handoverPlan_list.jsp"/>
			<forward name="list_already_approval" path="/WEB-INF/jsp/handoverPlan/list_already_approval.jsp"/>
			<forward name="add_handoverPlan" path="/WEB-INF/jsp/handoverPlan/add_handoverPlan.jsp"/>
			<forward name="upd_handoverPlan" path="/WEB-INF/jsp/handoverPlan/upd_handoverPlan.jsp"/>
			<forward name="handoverPlan_detail" path="/WEB-INF/jsp/handoverPlan/handoverPlan_detail.jsp"/>
			<forward name="handoverPlan_approval" path="/WEB-INF/jsp/handoverPlan/handoverPlan_approval.jsp"/>
			<!-- 需求57 轮岗计划表台账 -->
			<forward name="HandoverPlanLedgerList" path="/WEB-INF/jsp/handoverPlan/handoverPlan_ledger_list.jsp"></forward>
			<forward name="handoverPlan_ledger_detail" path="/WEB-INF/jsp/handoverPlan/handoverPlan_ledger_detail.jsp"></forward>
		</action>
