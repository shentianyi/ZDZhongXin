/* CSS Document */

.redFont p label span{color:red;}
.select {margin-top:10px;}
.main_right .right_t {padding:0px 0px 12px 24px;margin-top:76px;line-height:22px;color:#333;font-size:16px;font-family:"Microsoft YaHei";background:url(../images/check/check_bg.png) -106px -29px no-repeat;}
.main_right .check_detail {width:100%;}
.check_detail .detail_t {padding-left:8px;height:24px;line-height:24px;color:#333;background-color:#dfdfdf;border:#d2d2d2 1px solid;border-bottom:none;position:relative;}
.detail_t i.upi {width:24px;height:24px;display:block;overflow:hidden;background:url(../images/check/check_bg.png) -54px -58px no-repeat;position:absolute;top:0px;right:5px;cursor:pointer;}
.detail_t i.downi {background-position:-29px -58px;}
.detail_cont .spe_t {background-color:#f0f0f0;}
.detail_cont .detail_c {border-top:#d2d2d2 1px solid;}
.detail_cont .lastc {border-bottom:#e9e9e9 1px solid;}
.detail_cont .detail_line {height:24px;line-height:24px;border:#e9e9e9 1px solid;border-top:none;border-right:none;}
.detail_cont .lastline {border-bottom:none;}
.detail_cont .oddline {background-color:#f8f8f8;}
.detail_cont .noborder {border-bottom:none;}
.detail_line .line_p {height:24px;overflow:hidden;float:left;border-right:#e9e9e9 1px solid;}
.detail_line .tp {text-align:center;color:#626262;}
.detail_line .cpr {color:#999;padding-right:10px;text-align:right;}
.detail_line .cpl {color:#999;padding-left:6px;text-align:left;}
.line2sub_1 .tp {width:81px;}
.line2sub_1 .cpl {width:690px;}
.line8sub_1 .tp {width:81px;}
.line8sub_1 .cp1 {width:96px;}
.line8sub_1 .cp2 {width:108px;}
.line8sub_1 .cp3 {width:107px;}
.line6sub_1 .tp {width:115px;}
.line6sub_1 .cpr1 {width:133px;}
.line6sub_1 .cpr2 {width:132px;}
.line2sub_2 .tp {width:115px;}
.line2sub_2 .cpl {width:656px;}
.line4sub_3 .tp {width:81px;}
.line4sub_3 .cp1 {width:297px;}
.line4sub_3 .cp2 {width:296px;}
.line8sub_2 .tp1 {width:115px;}
.line8sub_2 .tp2 {width:97px;}
.line8sub_2 .tp3 {width:96px;}
.line8sub_2 .cpr {width:77px;}
.line4sub_2 .tp {width:115px;}
.line4sub_2 .cp1 {width:263px;}
.line4sub_2 .cp2 {width:262px;}
.line4sub_1 .tp1 {width:86px;}
.line4sub_1 .tp2 {width:67px;}
.line4sub_1 .cpr {width:56px;}
.line4sub_1 .cpl {width:350px;}
.line4sub_4 .tp {width:81px;}
.line4sub_4 .cp1 {width:490px;}
.line4sub_4 .cp2 {width:107px;}
.detail_line .redfont {color:#ff4d35;}
.detail_line .bluefont {color:#0084ff;}
.detail_line .greenfont {color:#209800;}


.main_right .check_list {width:100%;}
.check_list .check_tab {height:28px;padding:15px 0px 28px;background:url(../images/check/tab_bg.png) 0px 43px repeat-x;}
.check_tab li {width:60px;height:28px;line-height:28px;overflow:hidden;text-align:center;float:left;margin-right:1px;display:inline;color:#888;cursor:pointer;background:url(../images/check/check_bg.png) 0px 0px no-repeat;}
.check_tab li.curli {color:#cb8500;background-position:-61px 0px;}
.checklist_cont .list_main {width:98%;padding-left:9px;display:none;}
.list_main .lm_cont {width:177px;height:258px;margin:0px 13px 14px 0px;float:left;display:inline;position:relative;z-index:1;}
.lm_cont .abcont {width:175px;height:auto;padding-bottom:10px;border:#e6e6e6 1px solid;background-color:#f4f4f4;position:absolute;top:0px;left:0px;z-index:10;margin-right: 10px;}
.lm_cont .list_t {height:27px;line-height:27px;padding-left:8px;color:#666;background:url(../images/check/spe_line.png) 0px 25px repeat-x;}
.lm_cont .list_img {width:157px;height:157px;margin:7px auto 9px;border:#ccc 1px solid;}
.list_img img {width:155px;height:155px;border:#fff 1px solid;cursor:pointer;}
.lm_cont .list_line {width:175px;height:2px;overflow:hidden;margin:0 auto;background:url(../images/check/spe_line.png) 0px 0px repeat-x;}
.lm_cont .list_select {width:150px;height:30px;padding-left:7px;border:#d9d9d9 1px solid;margin:10px auto 0px;background:url(../images/check/check_bg.png) 0px -140px repeat-x;position:relative;padding-top:6px;}
.list_select .ls_cont {width:120px;height:30px;line-height:30px;overflow:hidden;color:#b5b5b5;text-align:left;float:left;}
.list_select .selectcont {color:#666;}
.list_select .select_a {width:28px;height:30px;float:right;background:url(../images/check/check_bg.png) 0px -58px no-repeat;}
.list_select .select_cont {position:absolute;top:30px;left:-1px;width:157px;border:#d9d9d9 1px solid;background-color:#fff;display:none;}
.select_cont li {width:150px;line-height:22px;padding-left:7px;color:#666;cursor:pointer;}
.select_cont li.hoverli {background-color:#fdc04d;color:#a16900;}
.lm_cont .list_tips {width:159px;margin:0 auto;line-height:16px;color:#666;padding-top:8px;display:none;}
.list_tips p {width:159px;height:32px;overflow:hidden;margin-bottom:8px;}
.lm_cont .list_area {width:159px;height:40px;margin:8px auto 0px;display:none;}
.list_area textarea {width:151px;height:32px;line-height:16px;overflow:hidden;padding:3px;border:#cecfd3 1px solid;font-size:12px;color:#666;resize:none;}
.checklist_cont .curcont {z-index:10;}
.curcont .abcont {border-color:#fdc04d;}
.curcont .list_t {color:#cb8500;background:url(../images/check/check_bg.png) 0px -112px repeat-x;}
.curcont .list_tips {display:block;}
.curcont .list_line {width:159px;}
.list_main .ln_btn {
	width:100%;
	float:left;
	margin-top:250px \9;
	*margin-top:250px;
	_margin-top:250px;
}

.ln_btn .inputbtn {width:100px;height:28px;line-height:28px;text-align:center;margin-right:20px;color:#cb8500;float:right;border:none;background:none;background:url(../images/check/check_bg.png) 0px -29px no-repeat;cursor:pointer;}
.relist .lm_cont {width:175px;border:#e6e6e6 1px solid;background-color:#f4f4f4;padding-bottom:10px;height:auto;min-height:260px;}
.relist .list_line {width:159px;}
.lm_cont .list_tips_re {width:159px;margin:0 auto;line-height:16px;color:#666;padding-top:8px;}
.list_tips_re p {width:159px;height:32px;overflow:hidden;margin-bottom:8px;}
.lm_cont .list_area_re {width:159px;height:40px;margin:8px 4px 0px;}
.list_area_re textarea {width:151px;height:32px;line-height:16px;overflow:hidden;padding:3px;border:#cecfd3 1px solid;font-size:12px;color:#666;resize:none;}
.lm_cont .list_radio {width:165px;height:87px;margin:10px auto 0px;}
.list_radio p {width:100%;height:22px;line-height:22px;}
.list_radio p input {width:13px;height:13px;overflow:hidden;margin:4px 0px 0px 6px;float:left;display:inline;}
.list_radio p label {width:140px;height:22px;color:#666;float:right;}
.list_radio p label span {height:22px;overflow:hidden;cursor:default;}

.list_radio p label .radioname {width:47px;float:left;}

.list_radio p label .radiocont {width:90px;float:left;}
.list_radio p label span.spela {color:#ff4d35;}
.lm_cont .list_checkbox {width:139px;height:36px;margin:8px auto;padding:0px 15px;background-color:#ebebeb;}
.list_checkbox label {width:110px;line-height:36px;float:left;color:#666;}
.list_checkbox input {width:13px;height:13px;float:right;margin-top:11px;display:inline;}
.relist .curcont {border-color:#fdc04d;}
.lm_cont .list_tips_load {width:159px;margin:0 auto;line-height:16px;color:#666;padding-top:8px;}
.list_tips_load p {width:159px;height:32px;overflow:hidden;margin-bottom:9px;}
.list_tips_load textarea {width:151px;height:32px;_height:30px;line-height:16px;padding:3px;overflow:hidden;border:#cecfd3 1px solid;font-size:12px;color:#666;resize:none;}
.uploadlist .lm_cont {height:280px;}
.uploadlist .list_select {display:none;margin-top:10px;}
.uploadlist .curcont .list_select {display:block;}
.lm_cont .list_tips_load .list_line {display:none;}
.curcont .list_tips_load .list_line {display:block;margin-top:8px;}
.uploadlist .list_img {position:relative;overflow:hidden;}
.list_img i {width:16px;height:16px;display:block;overflow:hidden;background:url(../images/check/check_bg.png) -79px -58px no-repeat;position:absolute;top:3px;right:3px;cursor:pointer;}
.list_img .fileinput {width:200px;height:155px;position:absolute;top:1px;right:1px;font-size:50px;filter:alpha(opacity=0);-moz-opacity:0;opacity:0;cursor:pointer;}

#layoutbg {display:none;}
*html,*html body {background-image:url(about:blank);background-attachment:fixed;}
#imgbox {width:960px;height:550px;margin-left:-550px;background-color:#fff;position:fixed;top:30px;left:54%;z-index:9999;display:none;
_position:absolute;/*IE6*/
_top:expression(eval(documentElement.scrollTop + 30));}
.box_img {width:780px;height:500px;overflow:hidden;float:left;margin:10px;display:inline;cursor:move;position:relative;}
.box_img img.boximg {position:absolute;cursor:move;z-index:10;}
/*.box_img .imgline  {width:100%;height:60px;line-height:30px;background-color:#000;filter:alpha(opacity=80);-moz-opacity:0.80;opacity:0.80;
position:absolute;bottom:0px;_bottom:-1px;left:0px;z-index:100;cursor:default;}*/
.box_img .imgline {padding-right:300px;line-height:30px;background-color:#000;filter:alpha(opacity=80);-moz-opacity:0.80;opacity:0.80;
position:absolute;bottom:0px;_bottom:-1px;left:0px;z-index:100;cursor:default;width:100%;}

.imgline span {margin:0px 20px;float:left;display:inline;color:#fff;cursor:pointer;}
.box_right .imgline {height:26px;line-height:26px;}
.box_left {width:155px;height:500px;float:left;display:inline;margin:10px 0px 0px 5px;}
.box_left p {width:120px;padding-left:5px;line-height:23px;overflow:hidden;cursor:pointer;}
.box_left p.curli_list {color:#fff;background:url(../images/check/curli_bg.png) 0px 0px no-repeat;}
.box_right {width:200px;height:167px;overflow:hidden;background-color:#fff;border:#fdc04d 4px solid;position:absolute;bottom:0px;right:0px;z-index:1000;}

#imgbox .box_close {height:22px;padding-right:10px;}
.box_close a {width:66px;height:22px;float:right;background:url(../images/check/check_bg.png) 0px -89px no-repeat;}




.jscroll-none{ position: absolute;height: 100%; top: 0px; left: 155px; overflow: hidden; z-index: 10000; width: 15px; background: url(../images/check/scroll_bg.png) -16px 0px repeat-y;}

