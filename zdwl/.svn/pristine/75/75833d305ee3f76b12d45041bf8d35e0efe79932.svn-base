package com.zd.csms.marketing.web.form;

import org.apache.struts.action.ActionForm;

import com.zd.csms.marketing.model.PaymentQueryVO;
import com.zd.csms.marketing.model.PaymentVO;

public class PaymentForm extends ActionForm{
	/**
	 * 
	 */
	private static final long serialVersionUID = 1L;
	
	private PaymentVO payment = new PaymentVO();
	private PaymentQueryVO query = new PaymentQueryVO();
	public PaymentVO getPayment() {
		return payment;
	}
	public void setPayment(PaymentVO payment) {
		this.payment = payment;
	}
	public PaymentQueryVO getQuery() {
		return query;
	}
	public void setQuery(PaymentQueryVO query) {
		this.query = query;
	}
	
}
