package com.zd.csms.marketing.web.form;

import org.apache.struts.action.ActionForm;

import com.zd.csms.marketing.model.DealerQueryVO;
import com.zd.csms.marketing.model.DealerVO;
import com.zd.csms.marketing.querybean.DealerQueryBean;

public class DealerForm extends ActionForm{

	/**
	 * 
	 */
	private static final long serialVersionUID = 1L;
	
	private DealerVO dealer = new DealerVO();
	private DealerQueryVO query = new DealerQueryVO();
	private DealerQueryBean dealerQ = new DealerQueryBean(); 

	public DealerVO getDealer() {
		return dealer;
	}

	public void setDealer(DealerVO dealer) {
		this.dealer = dealer;
	}

	public DealerQueryVO getQuery() {
		return query;
	}

	public void setQuery(DealerQueryVO query) {
		this.query = query;
	}

	public DealerQueryBean getDealerQ() {
		return dealerQ;
	}

	public void setDealerQ(DealerQueryBean dealerQ) {
		this.dealerQ = dealerQ;
	}

	
	
	
	
}
