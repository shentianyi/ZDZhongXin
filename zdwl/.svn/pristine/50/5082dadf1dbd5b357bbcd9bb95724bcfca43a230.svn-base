package com.zd.csms.finance.web.form;

import org.apache.struts.action.ActionForm;

import com.zd.csms.finance.model.OpenInvoiceListQueryVO;
import com.zd.csms.finance.model.OpenInvoiceListVO;

public class OpenInvoiceListForm extends ActionForm {

	/**
	 * 
	 */
	private static final long serialVersionUID = -8083425691122345714L;

	private OpenInvoiceListVO openInvoiceList = new OpenInvoiceListVO();
	private OpenInvoiceListQueryVO openInvoiceListquery = new OpenInvoiceListQueryVO();
	private String openid;
	
	public OpenInvoiceListVO getOpenInvoiceList() {
		return openInvoiceList;
	}
	public void setOpenInvoiceList(OpenInvoiceListVO openInvoiceList) {
		this.openInvoiceList = openInvoiceList;
	}
	public OpenInvoiceListQueryVO getOpenInvoiceListquery() {
		return openInvoiceListquery;
	}
	public void setOpenInvoiceListquery(OpenInvoiceListQueryVO openInvoiceListquery) {
		this.openInvoiceListquery = openInvoiceListquery;
	}
	public String getOpenid() {
		return openid;
	}
	public void setOpenid(String openid) {
		this.openid = openid;
	}
	
	
	
}
