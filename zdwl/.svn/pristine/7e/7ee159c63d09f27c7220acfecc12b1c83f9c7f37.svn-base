package com.zd.csms.business.queryBean;

import com.zd.csms.business.model.DraftVO;

public class DraftQueryBean extends DraftVO{
	/**
	 * 
	 */
	private static final long serialVersionUID = 1L;
	private String dealerName;
	private String bankName;
	private String brandName;
	
	private Double updDate;//最后修改时间(最晚一辆车出库的审批时间)
	private Double yycMoney;//已押车金额 经历过在库状态的车辆
	private Integer kcts;//库存台数  除了导入车辆和入库申请中的车辆
	private Double kcMoney;//库存金额 除了导入车辆和入库申请中的车辆
	private Double ckMoney;//敞口金额  //敞口金额设置公式=开票金额*（1-保证金比例）-回款金额
	private Double hkMoney;//回款金额  已出库车辆金额
	private Double wycMoney;//未押车金额  开票金额-已押车金额
	
	private String un_mortgagecar_money;
	public String getDealerName() {
		return dealerName;
	}
	public void setDealerName(String dealerName) {
		this.dealerName = dealerName;
	}
	public String getBankName() {
		return bankName;
	}
	public void setBankName(String bankName) {
		this.bankName = bankName;
	}
	public Double getYycMoney() {
		return yycMoney;
	}
	public void setYycMoney(Double yycMoney) {
		this.yycMoney = yycMoney;
	}
	public Integer getKcts() {
		return kcts;
	}
	public void setKcts(Integer kcts) {
		this.kcts = kcts;
	}
	public Double getKcMoney() {
		return kcMoney;
	}
	public void setKcMoney(Double kcMoney) {
		this.kcMoney = kcMoney;
	}
	public Double getCkMoney() {
		return ckMoney;
	}
	public void setCkMoney(Double ckMoney) {
		this.ckMoney = ckMoney;
	}
	public Double getHkMoney() {
		return hkMoney;
	}
	public void setHkMoney(Double hkMoney) {
		this.hkMoney = hkMoney;
	}
	public Double getWycMoney() {
		return wycMoney;
	}
	public void setWycMoney(Double wycMoney) {
		this.wycMoney = wycMoney;
	}
	public String getUn_mortgagecar_money() {
		return un_mortgagecar_money;
	}
	public void setUn_mortgagecar_money(String un_mortgagecar_money) {
		this.un_mortgagecar_money = un_mortgagecar_money;
	}
	public String getBrandName() {
		return brandName;
	}
	public void setBrandName(String brandName) {
		this.brandName = brandName;
	}
	public Double getUpdDate() {
		return updDate;
	}
	public void setUpdDate(Double updDate) {
		this.updDate = updDate;
	}
	
}
