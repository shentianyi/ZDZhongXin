html, body, div, span, iframe,
h1, h2, h3, h4, h5, h6, p, pre,
a, em, font, img, small, strong,
b, u, i, s, dl, dt, dd, ol, ul, li,
fieldset, form, label, input, textarea, select, option {
	padding: 0; 
	margin: 0;
}
body {
	min-width: 320px;
	font: 12px/1.6em 'Microsoft YaHei', SimSun, Arial, Tahoma;
	color: #000;
	background-color: #fefefe;
}
ol, ul, li {
	list-style: none outside none;
}
img, fieldset { 
	border: 0;
}
.clear {
	clear: both;
	height: 0;
	overflow: hidden;
	font-size: 0;
}
input, a, select, textarea {
	outline: none;
}
a:link, a:visited {
	text-decoration: none; 
	color: #000; 
}
a:hover {
	text-decoration: none; 
	color: #000; 
}
a:focus {
	text-decoration: none; 
	color: #000; 
}
.fl {
	float: left;
}
.fr {
	float: right;
}
.hide {
	display: none!important;
}
html {
	height: 100%;
}

/* 头部浮动
-------------------------------------------------- */
body > header:first-child {
	width: 100%;
	height: 48px;	
}
.header {
	position: fixed;
	top: 0;
	z-index: 9999;
	width: 100%;
	height: 48px;
	background-color: #ffcb05;
}
.header-main .header-line {
	position: relative;
	height: 30px;
	line-height: 30px;
	padding: 10px 12px 8px;
	text-align: center;
}
.header-main .header-line span.fl {
	position: absolute;
	top: 10px;
	left: 12px;
	display: block;
	float: none;
	width: 40px;
	height: 30px;
	background-image: url('../images/mobile/header-back.png');
	background-size: 10px auto;
	background-repeat: no-repeat;
	background-position: center center;
}
.header-main span {
	font-size: 18px;
}

/* 主要内容区域
-------------------------------------------------- */
.main {
	position: absolute;
	z-index: 999;
	width: 100%;
	background-color: #fefefe;
}
.main-top {
	top: 48px;
}
.main-bottom {
	bottom: 50px;
}
.main-area {
	padding-bottom: 50px;
}
.list-title{
	height: 46px;
	background-color: #efefef;
	padding-left: 15px;
	font-size: 14px;
	line-height: 46px;
}
/* 底部浮动
-------------------------------------------------- */
footer {
	position: fixed;
	bottom: 0;
	z-index: 9999;
	width: 100%;
	height: 48px;
	background-color: #ffcb05;
}
footer .footer {
	overflow: hidden;
}

/* dl dt dd内容展示
-------------------------------------------------- */
.dl-line {
	overflow: hidden;
	margin: 0 12px;
	padding: 5px 0;
	line-height: 20px;
	font-size: 15px;
	border-bottom: #999 1px solid;
}
.dl-line dt {
	float: left;
	height: 3%;
	margin-right: 10px;
}
.dl-line dd {
	overflow: hidden;
}
.dl-line dd a {
	position: relative;
	z-index: 999;
	color: red!important;
}
.main .filter-link {
	position: fixed;
	top: 75px;
	right: 10px;
	z-index: 100;
	display: block;
	width: 40px;
	height: 40px;
	line-height: 40px;
	text-align: center;	
	color: #e1e1e1!important;
	font-size: 14px;
	background-color: #e1e1e1;
	border-radius: 20px;
}
.main .filter-link i {
	position: absolute;
	top: 13px;
	right: 7px;
	display: block;
	width: 27px;
	height: 20px;
	background-image: url('../images/mobile/icon-filter.png');
	background-size: 100% auto;
	background-repeat: no-repeat;
}
.main .filter-clicked {
	color: #999!important;
}
.main .filter-clicked i {
	display: none;	
}
.gray-line {
	width: 100%;
	height: 10px;
	background-color: #e1e1e1;
}

/* 筛选弹出层
-------------------------------------------------- */
.popup-box,
.popup-box1 {
	position: fixed;
	top: 120px;
	left: 0;
	z-index: 1000;
	display: none;
	overflow-y: scroll;
	min-width: 100%;
	max-height: 60%;
}
.popup-box form,
.popup-box1 .popup-inner {
	margin: 0 10px;
	padding: 25px;
	background-color: rgba(102, 102, 102, 0.9);
}
.popup-box dl {
	width: 100%;
	height: 26px;
	line-height: 26px;
	margin-bottom: 15px;
}
.popup-box dl dt {
	float: left;
	width: 4em;
	height: 26px;	
	font-size: 15px;	
	color: #e1e1e1;
}
.popup-box dl dd {
	height: 26px;
	margin-left: 4em;
	font-size: 15px;
}
.popup-box dl.letter-size4 dt {
	width: 5em;
}
.popup-box dl.letter-size4 dd {
	margin-left: 5em;
}
.select-line {
	position: relative;	
	background: #fff;
    border-radius: 2px;
}
.select-line:before {
    position: absolute;
    top: 0;
    right: 0;
    z-index: 10;
    width: 25px;
    height: 100%;
    background-color: #999;
    border-radius: 0 2px 2px 0;
    content: ' ';
}
.select-line:after {
    position: absolute;
    top: 50%;
    right: 7px;
    z-index: 100;
    width: 10px;
    height: 6px;
    margin-top: -2px;
    background-size: 100% auto;
    background-position: left top;
    background-repeat: no-repeat;
    background-image: url(../images/mobile/select-icon.png);
    border-radius: 0;
    content: ' ';
}
.dl-line .select-line {
	border: #999 1px solid;
}
.dl-line .input-line {
	border: #999 1px solid;
}
.select-line select {
	position: absolute;
	top: 0;
	left: 0;
	z-index: 1000;
	width: 100%;
	height: 26px;
	line-height: 26px;
	border: none;
	background: transparent;
	opacity: 0;
}
.input-line {
	background: #fff;
    border-radius: 2px;
}
.input-line input,
.select-line input {
	width: 90%;
	padding: 0 5%;
	border: none;
	background: #fff;
}
.popup-box .btn-line {
	padding: 10px 20px 0;	
}
.btn-line button {
	width: 100%;
	height: 2em;	
	line-height: 2em;
	font-size: 18px;
	color: #fff;
	text-align: center;
	background-color: #000;
	border: none;
	border-radius: 5px;
}

/* 品牌弹出层
-------------------------------------------------- */
.popup-box-brand {
	position: absolute;
	top: 0;
	left: 0;
	z-index: 9999;
	display: none;
	overflow-y: scroll;
	width: 100%;
	background-color: #fff;
}
.car-brand-title {
	height: 20px;
	padding-left: 10px;
	line-height: 20px;
	background-color: #e1e1e1;
}
.car-brand-content {
	overflow: hidden;
}
.car-brand-content li {
	overflow: hidden;
	padding: 5px 0 5px 15px;
	line-height: 40px;
	font-size: 15px;
	border-bottom: #999 1px solid;
}
.car-brand-content li:last-child {
	border-bottom: none;
}
.car-brand-content li a {
	display: block;
	width: 100%;
}
.car-brand-content li img {
	width: 40px;
	height: 40px;
	margin-right: 15px;
}
.popup-brand-bar {
	position: fixed;
	right: 10px;
	z-index: 99999;
	width: 20px;
	background-color: #e1e1e1;
}
.popup-brand-bar li {
	width: 100%;
	height: 18px;
	line-height: 18px;
	font-size: 12px;
	text-align: center;
}

/* 按钮行  col1为1个按钮  col2为2个按钮
-------------------------------------------------- */
.button-line {
	position: relative;
	z-index: 999;
	overflow: hidden;
	width: 100%;
	margin: 20px 0 0;
}
.button-line li {
	float: left;
	height: 40px;
	margin-bottom: 10px;	
}
.button-line li a {
	display: block;
	height: 40px;
	margin: 0 12px;	
	line-height: 40px;
	text-align: center;
	color: #fff!important;
	font-size: 18px;
	background-color: #000;
	border-radius: 3px;
}
.col1 li {
	width: 100%;
}
.col2 li {
	width: 50%;
}

/* 页面水印
-------------------------------------------------- */
.main-repeat-box {
	position: relative;
}
.water-mark {
	position: absolute;
	top: 0;
	left: 0;
	z-index: 10;
	overflow:hidden;
	width: 100%;
	height: 100%;
	line-height: 160%;
	text-align: center;
	font-size: 200px;
	color: rgba(0, 0, 0, 0.2);
}

/* 媒体查询 3倍密度手机
-------------------------------------------------- */
@media only screen and (-webkit-min-device-pixel-ratio: 3) {
	body > header:first-child {
		height: 60px;
	}
	.header {
		height: 60px;
	}
	.main-top {
		top: 60px;
	}
    .dl-line {
    	margin: 0 15px;
    	padding: 7px 0;
    	line-height: 30px;
    	font-size: 19px;
    }
    footer {
    	height: 60px;
    }
    .main-bottom {
    	bottom: 60px;
    }
    .main-area {
    	padding-bottom: 62px;
    }
    .header-main .header-line {
    	height: 38px;
    	padding: 12px 15px 10px;
    	line-height: 38px;
    }
    .header-main .header-line a.logo {
    	 width: 120px;
    	 height: 38px;
    }
    .header-main .header-line span.fl {
		top: 12px;
		left: 15px;
		width: 50px;
		height: 38px;
		background-size: 13px auto;
	}
    .header-main span {
		font-size: 22px;
	}
    .select-line:before {
    	width: 35px;
    }
    .select-line:after {
    	right: 11px;
    	width: 13px;
    	height: 8px;
    }
    .button-line {
    	margin: 25px 0 0;
    }
    .button-line li {
    	height: 50px;
    }
    .button-line li a {
    	height: 50px;
    	margin: 0 15px;
    	line-height: 50px;
    	font-size: 22px;
    }
    .main .filter-link {
		top: 80px;
    	width: 54px;
    	height: 54px;
    	line-height: 54px;
    	font-size: 18px;
    	border-radius: 27px;
    }
    .main .filter-link i {
    	width: 35px;
    	height: 28px;
    	top: 16px;
    	left: 9px;
    }
	.popup-box, 
	.popup-box1 {
		top: 150px;
	}
    .popup-box form {
    	padding: 30px;
    }
    .popup-box dl {
    	height: 32px;
    	line-height: 32px;
    	margin-bottom: 20px;
    }
    .popup-box dl dt,
    .popup-box dl dd {
    	height: 32px;
    	font-size: 19px;
    }
    .input-line input {
    	font-size: 15px;
    }
    .popup-box .btn-line {
    	padding: 15px 30px 0;
    }
    .btn-line button {
    	font-size: 22px;
    }
    .gray-line {
    	height: 13px;
    }
    .car-histroy-list li a {
		height: 60px;
		font-size: 19px;
	}
	.car-brand-title {
		height: 26px;
		padding-left: 13px;
		line-height: 26px;
		font-size: 15px;
	}
	.car-brand-content li {
		padding: 7px 0 7px 18px;
		line-height: 50px;
		font-size: 19px;
	}
	.car-brand-content li img {
		width: 50px;
		height: 50px;
		margin-right: 18px;
	}
	.popup-brand-bar {
		width: 30px;
	}
	.popup-brand-bar li {
		height: 24px;
		line-height: 24px;
		font-size: 15px;
	}
	.water-mark {
		font-size: 260px;
	}
	.list-title{
		height: 60px;
		background-color: #efefef;
		padding-left: 15px;
		font-size: 18px;
		line-height: 60px;
	}
}
/* ios特殊处理
-------------------------------------------------- */
.platform-ios .header {
    padding-top: 20px;
}

.platform-ios .main-top {
    top: 68px;
}

.platform-ios .apply-header {
    top: 68px;
}
.platform-ios .car-header {
    top: 68px;
}

@media only screen and (-webkit-min-device-pixel-ratio: 3) {
    .platform-ios .main-top {
        top: 80px;
    }
	.platform-ios .apply-header {
    top: 80px;
	}
	.platform-ios .car-header {
    top: 80px;
	}
}