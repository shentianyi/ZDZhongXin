<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE struts-config PUBLIC "-//Apache Software Foundation//DTD Struts Configuration 1.1//EN" "http://jakarta.apache.org/struts/dtds/struts-config_1_1.dtd">

<struts-config>

	<form-beans>
		<form-bean name="marketApprovalForm" type="com.zd.csms.marketing.web.form.MarketApprovalForm"></form-bean>
		<form-bean name="mpcForm" type="com.zd.csms.marketing.web.form.MarketProjectCirculationForm"></form-bean>
		<form-bean name="dealerForm" type="com.zd.csms.marketing.web.form.DealerForm"></form-bean>
		<form-bean name="agreementSendForm" type="com.zd.csms.marketing.web.form.AgreementSendForm"></form-bean>
		<form-bean name="agreementBackForm" type="com.zd.csms.marketing.web.form.AgreementBackForm"></form-bean>
		<form-bean name="btForm" type="com.zd.csms.marketing.web.form.BusinessTransferForm"></form-bean>
		<form-bean name="mcForm" type="com.zd.csms.marketing.web.form.ModeChangeForm"></form-bean>
		<form-bean name="ecForm" type="com.zd.csms.marketing.web.form.ExpenseChangeForm"></form-bean>
		<form-bean name="bdForm" type="com.zd.csms.marketing.web.form.BindDealerForm"></form-bean>
		<form-bean name="unbdForm" type="com.zd.csms.marketing.web.form.UnBindDealerForm"></form-bean>
		<form-bean name="deForm" type="com.zd.csms.marketing.web.form.DealerExitForm"></form-bean>
		<form-bean name="supervisePayForm" type="com.zd.csms.marketing.web.form.SupervisePayForm"></form-bean>
		<form-bean name="superviseArrearsForm" type="com.zd.csms.marketing.web.form.SuperviseArrearsForm"></form-bean>
		<form-bean name="invoiceForm" type="com.zd.csms.marketing.web.form.InvoiceForm"></form-bean>
		<form-bean name="paymentForm" type="com.zd.csms.marketing.web.form.PaymentForm"></form-bean>
		<form-bean name="refundForm" type="com.zd.csms.marketing.web.form.DealerRefundForm"></form-bean>
		<form-bean name="distribsetForm" type="com.zd.csms.set.web.form.DistribsetForm"></form-bean>
		<form-bean name="brandForm" type="com.zd.csms.marketing.web.form.BrandForm"></form-bean>
	</form-beans>
	
  <global-forwards>
    <forward name="error" path="/common/500error.jsp" />
  </global-forwards> 
  
	<action-mappings>
		<action path="/market/marketApproval" 
		    name="marketApprovalForm" 
		    parameter="method" 
		    type="com.zd.csms.marketing.web.action.MarketApprovalAction"
		    scope="request">
			<forward name="marketApprovalList" path="/WEB-INF/jsp/marketApproval.jsp"/>
		</action>
		
		<action path="/market/projectCirculationForm" 
		    name="mpcForm" 
		    parameter="method" 
		    type="com.zd.csms.marketing.web.action.ProjectCirculationAction"
		    scope="request">
			<forward name="list_not_approval" path="/WEB-INF/jsp/market/projectCirculation/list_not_approval.jsp"/>
			<forward name="list_already_approval" path="/WEB-INF/jsp/market/projectCirculation/list_already_approval.jsp"/>
			<forward name="add" path="/WEB-INF/jsp/market/projectCirculation/add.jsp"/>
			<forward name="update" path="/WEB-INF/jsp/market/projectCirculation/update.jsp"/>
			<forward name="approval" path="/WEB-INF/jsp/market/projectCirculation/approval.jsp"/>
			<forward name="detail" path="/WEB-INF/jsp/market/projectCirculation/detailPage.jsp"/>
			<!-- 项目进驻流转单台账及详情 -->
			<forward name="marketProjectList_ledger" path="/WEB-INF/jsp/market/projectCirculation/marketProjectList_ledger.jsp"></forward>
			<forward name="detailLedger" path="/WEB-INF/jsp/market/projectCirculation/detailLedger.jsp"></forward>
		</action>
		
		<action path="/market/dealer" 
		    name="dealerForm" 
		    parameter="method" 
		    type="com.zd.csms.marketing.web.action.DealerAction"
		    scope="request">
			<forward name="dealerList" path="/WEB-INF/jsp/market/dealer/list.jsp"/>
			<!-- 监管费管理台账 -->
			<forward name="expenseTotalList" path="/WEB-INF/jsp/market/dealer/expenseTotal.jsp"/>
			<forward name="detailInfo" path="/WEB-INF/jsp/market/dealer/detailPage.jsp"/>
			<!-- 缴费详情列表 -->
			<forward name="paymentDetailList" path="/WEB-INF/jsp/market/dealer/paymentDetailList.jsp"/>
			<forward name="selectDealerList" path="/WEB-INF/jsp/market/common/selectDealerList.jsp"/>
		</action>
		
		<action path="/market/businessTransfer" 
		    name="btForm" 
		    parameter="method" 
		    type="com.zd.csms.marketing.web.action.BusinessTransferAction"
		    scope="request">
			<forward name="list_wait_approval" path="/WEB-INF/jsp/market/businessTransfer/list_wait_approval.jsp"/>
			<forward name="list_already_approval" path="/WEB-INF/jsp/market/businessTransfer/list_already_approval.jsp"/>
			<forward name="add" path="/WEB-INF/jsp/market/businessTransfer/add.jsp"/>
			<forward name="update" path="/WEB-INF/jsp/market/businessTransfer/update.jsp"/>
			<forward name="update_zp" path="/WEB-INF/jsp/market/businessTransfer/update_zp.jsp"/>
			<forward name="update_yw" path="/WEB-INF/jsp/market/businessTransfer/update_yw.jsp"/>
			<forward name="detailPage" path="/WEB-INF/jsp/market/businessTransfer/detailPage.jsp"/>
			
		</action>
		
		<action path="/market/modeChange" 
		    name="mcForm" 
		    parameter="method" 
		    type="com.zd.csms.marketing.web.action.ModeChangeAction"
		    scope="request">
			<forward name="list_wait_approval" path="/WEB-INF/jsp/market/modeChange/list_wait_approval.jsp"/>
			<forward name="list_already_approval" path="/WEB-INF/jsp/market/modeChange/list_already_approval.jsp"/>
			<forward name="add" path="/WEB-INF/jsp/market/modeChange/add.jsp"/>
			<forward name="update" path="/WEB-INF/jsp/market/modeChange/update.jsp"/>
			<forward name="detailPage" path="/WEB-INF/jsp/market/modeChange/detailPage.jsp"/>
			<forward name="approval" path="/WEB-INF/jsp/market/modeChange/approval.jsp"/>
		</action>
		
		<action path="/market/expenseChange" 
		    name="ecForm" 
		    parameter="method" 
		    type="com.zd.csms.marketing.web.action.ExpenseChangeAction"
		    scope="request">
			<forward name="list_wait_approval" path="/WEB-INF/jsp/market/expenseChange/list_wait_approval.jsp"/>
			<forward name="list_already_approval" path="/WEB-INF/jsp/market/expenseChange/list_already_approval.jsp"/>
			<forward name="add" path="/WEB-INF/jsp/market/expenseChange/add.jsp"/>
			<forward name="update" path="/WEB-INF/jsp/market/expenseChange/update.jsp"/>
			<forward name="detailPage" path="/WEB-INF/jsp/market/expenseChange/detailPage.jsp"/>
			<forward name="approval" path="/WEB-INF/jsp/market/expenseChange/approval.jsp"/>
			<forward name="list_management_ledger" path="/WEB-INF/jsp/market/expenseChange/list_management_ledger.jsp"/><!-- 监管费变更管理台账 -->
		</action>
		
		<action path="/market/bindDealer" 
		    name="bdForm" 
		    parameter="method" 
		    type="com.zd.csms.marketing.web.action.BindDealerAction"
		    scope="request">
			<forward name="list_wait_approval" path="/WEB-INF/jsp/market/bindDealer/list_wait_approval.jsp"/>
			<forward name="list_already_approval" path="/WEB-INF/jsp/market/bindDealer/list_already_approval.jsp"/>
			<forward name="add" path="/WEB-INF/jsp/market/bindDealer/add.jsp"/>
			<forward name="update" path="/WEB-INF/jsp/market/bindDealer/update.jsp"/>
			<forward name="update_zp" path="/WEB-INF/jsp/market/bindDealer/update_zp.jsp"/>
			<forward name="update_yw" path="/WEB-INF/jsp/market/bindDealer/update_yw.jsp"/>
			<forward name="detailPage" path="/WEB-INF/jsp/market/bindDealer/detailPage.jsp"/>
			<forward name="approval" path="/WEB-INF/jsp/market/bindDealer/approval.jsp"/>
			<!-- 经销商/金融机构绑定台账 -->
			<forward name="dfi_bind_ledger" path="/WEB-INF/jsp/market/bindDealer/dfi_bind_ledger.jsp"></forward>
			<forward name="detail_bind_ledger" path="/WEB-INF/jsp/market/bindDealer/detail_bind_ledger.jsp"></forward>
		</action>
		
		<action path="/market/unbindDealer" 
		    name="unbdForm" 
		    parameter="method" 
		    type="com.zd.csms.marketing.web.action.UnBindDealerAction"
		    scope="request">
			<forward name="list_wait_approval" path="/WEB-INF/jsp/market/unbindDealer/list_wait_approval.jsp"/>
			<forward name="list_already_approval" path="/WEB-INF/jsp/market/unbindDealer/list_already_approval.jsp"/>
			<forward name="add" path="/WEB-INF/jsp/market/unbindDealer/add.jsp"/>
			<forward name="update" path="/WEB-INF/jsp/market/unbindDealer/update.jsp"/>
			<forward name="update_zp" path="/WEB-INF/jsp/market/unbindDealer/update_zp.jsp"/>
			<forward name="update_yw" path="/WEB-INF/jsp/market/unbindDealer/update_yw.jsp"/>
			<forward name="detailPage" path="/WEB-INF/jsp/market/unbindDealer/detailPage.jsp"/>
			<forward name="approval" path="/WEB-INF/jsp/market/unbindDealer/approval.jsp"/>
			<!-- 经销商/金融机构拆绑台账 -->
			<forward name="dfi_ledger" path="/WEB-INF/jsp/market/unbindDealer/dfi_ledger.jsp"></forward>
			<forward name="detailLedgerPage" path="/WEB-INF/jsp/market/unbindDealer/detailLedgerPage.jsp"></forward>
		</action>
		
		<action path="/market/dealerExit" 
		    name="deForm" 
		    parameter="method" 
		    type="com.zd.csms.marketing.web.action.DealerExitAction"
		    scope="request">
			<forward name="list_wait_approval" path="/WEB-INF/jsp/market/dealerExit/list_wait_approval.jsp"/>
			<forward name="list_already_approval" path="/WEB-INF/jsp/market/dealerExit/list_already_approval.jsp"/>
			<forward name="add" path="/WEB-INF/jsp/market/dealerExit/add.jsp"/>
			<forward name="update" path="/WEB-INF/jsp/market/dealerExit/update.jsp"/>
			<forward name="update_zp" path="/WEB-INF/jsp/market/dealerExit/update_zp.jsp"/>
			<forward name="update_yw" path="/WEB-INF/jsp/market/dealerExit/update_yw.jsp"/>
			<forward name="update_cw" path="/WEB-INF/jsp/market/dealerExit/update_cw.jsp"/>
			<forward name="detailPage" path="/WEB-INF/jsp/market/dealerExit/detailPage.jsp"/>
			<forward name="approval" path="/WEB-INF/jsp/market/dealerExit/approval.jsp"/>
			<!-- 经销商撤店台账 -->
			<forward name="DealerWithdrawalLedger" path="/WEB-INF/jsp/market/dealerExit/DealerWithdrawalLedger.jsp"></forward>
			<forward name="detailLedgerPage" path="/WEB-INF/jsp/market/dealerExit/detailLedgerPage.jsp"></forward>
		</action>
		
		<action path="/market/invoice" 
		    name="invoiceForm" 
		    parameter="method" 
		    type="com.zd.csms.marketing.web.action.InvoiceAction"
		    scope="request">
			<forward name="list_wait_approval" path="/WEB-INF/jsp/market/invoice/list_wait_approval.jsp"/>
			<forward name="list_already_approval" path="/WEB-INF/jsp/market/invoice/list_already_approval.jsp"/>
			<forward name="add" path="/WEB-INF/jsp/market/invoice/add.jsp"/>
			<forward name="update" path="/WEB-INF/jsp/market/invoice/update.jsp"/>
			<forward name="detailPage" path="/WEB-INF/jsp/market/invoice/detailPage.jsp"/>
			<forward name="approval" path="/WEB-INF/jsp/market/invoice/approval.jsp"/>
			<forward name="list_draftVote_list" path="/WEB-INF/jsp/market/invoice/list_draftVote_list.jsp"/><!-- 监管费发票管理台账 -->
		</action>
		
		<action path="/market/payment" 
		    name="paymentForm" 
		    parameter="method" 
		    type="com.zd.csms.marketing.web.action.PaymentAction"
		    scope="request">
			<forward name="list_wait_approval" path="/WEB-INF/jsp/market/payment/list_wait_approval.jsp"/>
			<forward name="list_already_approval" path="/WEB-INF/jsp/market/payment/list_already_approval.jsp"/>
			<forward name="add" path="/WEB-INF/jsp/market/payment/add.jsp"/>
			<forward name="update" path="/WEB-INF/jsp/market/payment/update.jsp"/>
			<forward name="detailPage" path="/WEB-INF/jsp/market/payment/detailPage.jsp"/>
			<forward name="approval" path="/WEB-INF/jsp/market/payment/approval.jsp"/>
		</action>
		
		<action path="/market/refund" 
		    name="refundForm" 
		    parameter="method" 
		    type="com.zd.csms.marketing.web.action.DealerRefundAction"
		    scope="request">
			<forward name="list_wait_approval" path="/WEB-INF/jsp/market/refund/list_wait_approval.jsp"/>
			<forward name="list_already_approval" path="/WEB-INF/jsp/market/refund/list_already_approval.jsp"/>
			<forward name="add" path="/WEB-INF/jsp/market/refund/add.jsp"/>
			<forward name="update" path="/WEB-INF/jsp/market/refund/update.jsp"/>
			<forward name="detailPage" path="/WEB-INF/jsp/market/refund/detailPage.jsp"/>
			<forward name="approval" path="/WEB-INF/jsp/market/refund/approval.jsp"/>
			<forward name="supervise_refund_list" path="/WEB-INF/jsp/market/refund/supervise_refund_list.jsp"/><!--监管费退费管理台账  -->
		</action>
		
		<action path="/agreementSend" 
		    name="agreementSendForm" 
		    parameter="method" 
		    type="com.zd.csms.marketing.web.action.AgreementSendAction"
		    scope="request">
			<forward name="agreement_send_list" path="/WEB-INF/jsp/market/agreementSend/agreement_send_list.jsp"/>
			<forward name="add_agreement_send" path="/WEB-INF/jsp/market/agreementSend/add_agreement_send.jsp"/>
			<forward name="upd_agreement_send" path="/WEB-INF/jsp/market/agreementSend/upd_agreement_send.jsp"/>
		</action>
		<action path="/agreementBack" 
		    name="agreementBackForm" 
		    parameter="method" 
		    type="com.zd.csms.marketing.web.action.AgreementBackAction"
		    scope="request">
			<forward name="agreement_back_list" path="/WEB-INF/jsp/market/agreementBack/agreement_back_list.jsp"/>
			<forward name="add_agreement_back" path="/WEB-INF/jsp/market/agreementBack/add_agreement_back.jsp"/>
			<forward name="upd_agreement_back" path="/WEB-INF/jsp/market/agreementBack/upd_agreement_back.jsp"/>
		</action>
		<action path="/supervisePay" 
		    name="supervisePayForm" 
		    parameter="method" 
		    type="com.zd.csms.marketing.web.action.SupervisePayAction"
		    scope="request">
			<forward name="supervise_pay_list" path="/WEB-INF/jsp/market/supervisePay/supervise_pay_list.jsp"/>
			<forward name="add_supervise_pay" path="/WEB-INF/jsp/market/supervisePay/add_supervise_pay.jsp"/>
			<forward name="upd_supervise_pay" path="/WEB-INF/jsp/market/supervisePay/upd_supervise_pay.jsp"/>
		</action>
		<action path="/superviseArrears" 
		    name="superviseArrearsForm" 
		    parameter="method" 
		    type="com.zd.csms.marketing.web.action.SuperviseArrearsAction"
		    scope="request">
			<forward name="supervise_arrears_list" path="/WEB-INF/jsp/market/superviseArrears/supervise_arrears_list.jsp"/>
			<forward name="add_supervise_arrears" path="/WEB-INF/jsp/market/superviseArrears/add_supervise_arrears.jsp"/>
			<forward name="upd_supervise_arrears" path="/WEB-INF/jsp/market/superviseArrears/upd_supervise_arrears.jsp"/>
		</action>
		<action path="/agreementLedger" 
		    name="agreementBackForm" 
		    parameter="method" 
		    type="com.zd.csms.marketing.web.action.AgreementLedgerAction"
		    scope="request">
			<forward name="agreement_ledger_list" path="/WEB-INF/jsp/market/agreementBack/agreement_ledger_list.jsp"/>
		</action>
		<action path="/distribset" 
		    name="distribsetForm" 
		    parameter="method" 
		    type="com.zd.csms.set.web.action.DistribsetAction"
		    scope="request">
			<forward name="distrib_set" path="/WEB-INF/jsp/market/set/distrib_set.jsp"/>
			<!-- 经销商名录表-扩展参数设置：业务部 -->
			<forward name="extend_distrib_set" path="/WEB-INF/jsp/market/set/extend_distrib_set.jsp"/>
		</action>
		
		<action path="/market/warring" 
		    parameter="method" 
		    type="com.zd.csms.marketing.warning.WarringAction"
		    scope="request">
			<forward name="agreementExpires30" path="/WEB-INF/jsp/market/warring/agreementExpires30.jsp"/>
			<forward name="agreementExpires7" path="/WEB-INF/jsp/market/warring/agreementExpires7.jsp"/>
			<forward name="agreementOverdue" path="/WEB-INF/jsp/market/warring/agreementOverdue.jsp"/>
			<forward name="inNotPay" path="/WEB-INF/jsp/market/warring/inNotPay.jsp"/>
			<forward name="notApplyDate" path="/WEB-INF/jsp/market/warring/notApplyDate.jsp"/>
			<forward name="payDateBefore15" path="/WEB-INF/jsp/market/warring/payDateBefore15.jsp"/>
			<forward name="payDateBefore30" path="/WEB-INF/jsp/market/warring/payDateBefore30.jsp"/>
			<forward name="payDateBefore7" path="/WEB-INF/jsp/market/warring/payDateBefore7.jsp"/>
			<forward name="payDateExpire" path="/WEB-INF/jsp/market/warring/payDateExpire.jsp"/>
		</action>
		
		<action path="/brand" 
		    name="brandForm" 
		    parameter="method" 
		    type="com.zd.csms.marketing.web.action.BrandAction"
		    scope="request">
			<forward name="brand_list" path="/WEB-INF/jsp/market/brand/brand_list.jsp"/>
			<forward name="add_brand" path="/WEB-INF/jsp/market/brand/add_brand.jsp"/>
			<forward name="upd_brand" path="/WEB-INF/jsp/market/brand/upd_brand.jsp"/>
		</action>
	</action-mappings>
	
	
	
</struts-config>