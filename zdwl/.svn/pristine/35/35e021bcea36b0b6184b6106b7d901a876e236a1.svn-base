package com.zd.csms.supervisorymanagement.contants;

public enum HandoverTypeContants {
	
	/**
	 * 辞职
	 */
	RESIGN(1,"绑定/拆绑交接"),
	
	/**
	 * 辞退
	 */
	DISMISS(2,"轮岗交接"),
	
	/*
	 * 新增于20170516 -- 需求46
	*/
	DisMantLingShop(3,"撤店交接");
	
	private HandoverTypeContants(int code, String name) {
		this.code = code;
		this.name = name;
	}
	private int code;
	private String name;
	public int getCode() {
		return code;
	}
	public String getName() {
		return name;
	}
	
}
