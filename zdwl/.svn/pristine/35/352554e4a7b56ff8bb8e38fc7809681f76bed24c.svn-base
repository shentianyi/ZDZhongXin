package com.zd.csms.marketing.web.form;

import org.apache.struts.action.ActionForm;

import com.zd.csms.marketing.model.InvoiceQueryVO;
import com.zd.csms.marketing.model.InvoiceVO;

public class InvoiceForm extends ActionForm{
	/**
	 * 
	 */
	private static final long serialVersionUID = 1L;
	private InvoiceVO invoice = new InvoiceVO();
	private InvoiceQueryVO query = new InvoiceQueryVO();
	
	public InvoiceVO getInvoice() {
		return invoice;
	}
	public void setInvoice(InvoiceVO invoice) {
		this.invoice = invoice;
	}
	public InvoiceQueryVO getQuery() {
		return query;
	}
	public void setQuery(InvoiceQueryVO query) {
		this.query = query;
	}
	
	
}
