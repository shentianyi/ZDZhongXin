package com.zd.csms.message.web.form;

import org.apache.struts.action.ActionForm;

import com.zd.csms.message.model.MessageInfoVO;
import com.zd.csms.message.model.MessageQueryVO;
import com.zd.csms.message.model.MessageVO;
import com.zd.csms.message.model.SupMaMsgQueryVO;

public class MessageForm extends ActionForm {

	/**
	 * 
	 */
	private static final long serialVersionUID = -2542461743563151318L;
	private MessageVO message = new MessageVO();
	private MessageInfoVO messageInfo = new MessageInfoVO();
	private MessageQueryVO messagequery = new MessageQueryVO();
	private SupMaMsgQueryVO supMaQuery=new SupMaMsgQueryVO();
	public MessageVO getMessage() {
		return message;
	}
	public void setMessage(MessageVO message) {
		this.message = message;
	}
	public MessageInfoVO getMessageInfo() {
		return messageInfo;
	}
	public void setMessageInfo(MessageInfoVO messageInfo) {
		this.messageInfo = messageInfo;
	}
	public MessageQueryVO getMessagequery() {
		return messagequery;
	}
	public void setMessagequery(MessageQueryVO messagequery) {
		this.messagequery = messagequery;
	}
	public SupMaMsgQueryVO getSupMaQuery() {
		return supMaQuery;
	}
	public void setSupMaQuery(SupMaMsgQueryVO supMaQuery) {
		this.supMaQuery = supMaQuery;
	}
	
}